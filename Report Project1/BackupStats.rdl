  <DataSets>
    <DataSet Name="dsBackups">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with info as (
select 
d.name
,d.create_date [Created]
,DATABASEPROPERTYEX(d.name,'recovery') [RecoveryModel]
,case 
	when bs.type='D' then 'FULL' 
	when bs.type='I' then 'DIFF' 
	when bs.type='L' then 'LOG'
	when bs.type='F' then 'File or filegroup' 
	when bs.type='G' then 'Differential file'
	when bs.type='P' then 'Partial'
	when bs.type='Q' then 'Differential partial' 
	else bs.type end [BackupType]
,max(bs.backup_start_date) [LastBackup]
,max(rh.restore_date) [LastRestore]
from 
	sys.databases d
left join msdb.dbo.backupset bs on bs.database_name=d.name
left join msdb..restorehistory rh on bs.database_name=rh.destination_database_name 
where d.database_id&lt;&gt;2
group by d.name,bs.type,d.create_date
)
select 
	[name]
	,DATABASEPROPERTYEX(name,'Status') [status]
	,RecoveryModel
	,Created
	,LastRestore
	,[FULL],[DIFF],[LOG],[File or filegroup],[Differential file],[Partial],[Differential partial],[Unknown]
	,case 
		when ([LOG] is null and RecoveryModel='FULL') then 'Absent Log' 
		when ([LOG]&lt;dateadd(dd,-1,convert(date,getdate())) and RecoveryModel='FULL') then 'Old Log'
		when ([FULL] is null) then 'Absent Full'
		when ([FULL] &lt;dateadd(dd,-7,convert(date,getdate()))) then 'Old Full'
		else Null
	end [Note]
from info 
pivot (max(LastBackup) for BackupType in ([FULL],[DIFF],[LOG],[File or filegroup],[Differential file],[Partial],[Differential partial],[Unknown])) as t
option (recompile,maxdop 1)</CommandText>
      </Query>
