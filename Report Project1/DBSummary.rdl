    <DataSet Name="DBGrowthSummary">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with t1(database_name,dt,DBSizeMB,DataMB,LogMB) as 
	(
	SELECT  bs.database_name
			,bs.backup_start_date [dt]
			,sum(bf.file_size/1024/1024) [DBSizeMB]
			,(SELECT sum(bf2.file_size) FROM msdb..backupfile bf2 WHERE bf2.backup_set_id=bs.backup_set_id AND bf2.file_type='D')/1024/1024 [DataMB]
			,(SELECT sum(bf2.file_size) FROM msdb..backupfile bf2 WHERE bf2.backup_set_id=bs.backup_set_id AND bf2.file_type='L')/1024/1024 [LogMB]
	FROM  msdb..backupset bs
		JOIN msdb..backupfile bf ON bs.backup_set_id=bf.backup_set_id
	WHERE 
	  bs.type in ('D','L')

	GROUP BY 
		bs.backup_start_date
		,bs.backup_set_id
		,bs.database_name
	)
,t2(database_name,min_dt,max_dt) as 
(
select 
	database_name 
	,convert(datetime,min(dt))
	,convert(datetime,max(dt))
from t1 a 
group by database_name 
)

select 
	a.database_name
	,b.dt [PeriodStart]
	,c.dt  [PeriodEnd]
	,c.DBSizeMB-b.DBSizeMB [DBSizeGrowth_TTL]
	,c.DataMB-b.DataMB [DBSizeGrowth_DATA]
	,c.LogMB-b.LogMB [DBSizeGrowth_LOG]
	,(c.DBSizeMB-b.DBSizeMB)/c.DBSizeMB [PercentOfDB]
from t2 a 
	join t1 b on a.database_name=b.database_name and b.dt=a.min_dt
	join t1 c on a.database_name=c.database_name and c.dt=a.max_dt
where c.DBSizeMB-b.DBSizeMB&lt;&gt;0
order by (c.DBSizeMB-b.DBSizeMB) desc
option (recompile, maxdop 1)</CommandText>
      </Query>
      <Fields>
        <Field Name="database_name">
          <DataField>database_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PeriodStart">
          <DataField>PeriodStart</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PeriodEnd">
          <DataField>PeriodEnd</DataField>
