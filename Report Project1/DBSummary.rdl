        <CommandText>select 
	count(1) [DatabasesTotal]
	, (select convert(numeric(10,2),sum(mf.size)/128.) from sys.master_files mf (NOLOCK) where database_id&lt;5) [SystemDBSizeMB]
	, (select convert(numeric(10,2),sum(mf.size)/128.) from sys.master_files mf (NOLOCK) where database_id&gt;5) [UserDBSizeMB]
