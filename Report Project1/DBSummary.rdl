    <DataSet Name="DBGrowthEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with t1 as 
	(
	SELECT  row_number() over (partition by bs.database_name 
								order by bs.database_name,
										CONVERT(VARCHAR(20),bs.backup_start_date,112)) [id],
			bs.database_name
			,CONVERT(VARCHAR(20),bs.backup_start_date,106) [dt]
			,sum(bf.file_size/1024/1024) [DBSizeMB]
			,(SELECT sum(bf2.file_size) FROM msdb..backupfile bf2 WHERE bf2.backup_set_id=bs.backup_set_id AND bf2.file_type='D')/1024/1024 [DataMB]
			,(SELECT sum(bf2.file_size) FROM msdb..backupfile bf2 WHERE bf2.backup_set_id=bs.backup_set_id AND bf2.file_type='L')/1024/1024 [LogMB]
			,max(bs.backup_set_id) [bsid]
			,bs.type
	FROM  msdb..backupset bs
		JOIN msdb..backupfile bf ON bs.backup_set_id=bf.backup_set_id
	WHERE 
	  --bs.type='D'
	  bs.type in ('D','L')

	GROUP BY 
		bs.backup_start_date
		,bs.backup_set_id
		,bs.database_name
		,bs.type
	)
select 
	--ROW_NUMBER() over (order by abs(t.DBSizeMB-t1.DBSizeMB) desc) [ID]
	--,
	t1.database_name
	,t1.dt+' - '+t.dt [Period]
	,t1.type+' - '+t.type [backups]
--	,t1.id,t.id,t1.bsid,t.bsid
	,convert(numeric(20,2),t.DBSizeMB-t1.DBSizeMB) [DBGrowthMB]
	,convert(numeric(20,2),t.DataMB-t1.DataMB) [DataFileGrowthMB]
	,convert(numeric(20,2),t.LogMB-t1.LogMB) [LogFileGrowthMB]
	,convert(numeric(20,2),t1.DBSizeMB) [FirstSize]
	,convert(numeric(20,2),t.DBSizeMB) [LastSize]
from t1 join t1 t on t1.database_name=t.database_name and t1.id+1=t.id
where t.DBSizeMB-t1.DBSizeMB &lt;&gt; 0
order by t1.bsid desc
option (recompile)</CommandText>
      </Query>
