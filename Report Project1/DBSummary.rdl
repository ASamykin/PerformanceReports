    <DataSet Name="DBInfoCommon">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	d.database_id
	, d.name
	, suser_sname(d.owner_sid) [DB Owner]
--	, convert(varchar(20),d.create_date,106) + ' '+  convert(varchar(20),create_date,108) [CreateDate]
,create_date [CreateDate]
	, (select convert(numeric(10,0),sum(mf.size)/128.) from sys.master_files mf where mf.database_id=d.database_id) [DBSizeMB]
	 
	, d.recovery_model_desc [Recovery]
	, d.collation_name [Collation]
	, d.state_desc [State]
	, d.user_access_desc
	, d.snapshot_isolation_state_desc [Snaphot]
	, case when d.is_broker_enabled=1 then 'Enabled' else 'Disabled' end [ServiceBroker]
	, d.compatibility_level
	, d.page_verify_option_desc [PageVerify]
	, case when d.is_auto_close_on = 1 then 'YES' else 'no' end [AutoClose]
	, case when d.is_auto_shrink_on = 1 then 'YES' else 'no' end [AutoShrink]
	, case when d.is_trustworthy_on = 1 then 'YES' else 'no' end [TrustWorthyOn]
	, d.is_encrypted [Encrypted]
	, d.is_published [Published]
	, d.is_subscribed [Subscribed]
	, case when dm.mirroring_role is not NULL then 'YES' else 'no' end [Mirrored]
	, (select convert(numeric(10,2),sum(size)/128.) from tempdb.sys.database_files) [Tempdb]
,case when source_database_id is not null then db_name(source_database_id) else Null end [SnapshotOf]
from sys.databases d (NOLOCK)
join sys.database_mirroring dm (NOLOCK) on dm.database_id=d.database_id
order by d.database_id
option (maxdop 1, recompile)</CommandText>
      </Query>
