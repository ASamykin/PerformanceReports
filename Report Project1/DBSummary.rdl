	, sum(convert(tinyint,is_subscribed)) [Subscribed]
	, (select count(1) from sys.database_mirroring (NOLOCK) where mirroring_role is not NULL) [Mirrored]
	, sum(case when source_database_id is not null then 1 else 0 end) [Snapshots]
from sys.databases (NOLOCK)
option (maxdop 1, recompile)</CommandText>
