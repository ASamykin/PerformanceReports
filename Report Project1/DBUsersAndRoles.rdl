  <DataSets>
    <DataSet Name="dsUsers">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@check_db">
            <Value>=Parameters!DB.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @check_db varchar(128)
--select @check_db=''

create table #perms 
([database] nvarchar(128) not null,
[user] sysname not null,
[status] varchar(20) not null,
[type_desc] nvarchar(60) not null,
[public] tinyint,
[db_owner]  tinyint,
[db_accessadmin] tinyint,
[db_backupoperator] tinyint,
[db_datareader] tinyint,
[db_datawriter] tinyint,
[db_ddladmin] tinyint,
[db_denydatareader] tinyint,
[db_denydatawriter] tinyint,
[db_executor] tinyint,
[db_securityadmin] tinyint,
[other_roles] varchar(4096),
[other_permissions] varchar(4096)
)


declare @cmd varchar(8000)
declare @tmp varchar(8000)
declare @db varchar(255)
declare @compat smallint
declare @dbs table (name varchar(255),compat smallint)

--declare @cmd varchar(8000)
select @cmd='use [placeholder];
with u as (
		/*DB USERS*/
		select 
			case when isnull(suser_sname(sid),pri.name)= pri.name then pri.name else suser_sname(sid)+'' ''+quotename(pri.name) end [user]
			,pri.type_desc  
			,case when isnull(perm.permission_name,''no'')=''no'' then ''disabled'' else ''enabled'' end [status]
			,''public'' [role]
		from sys.database_principals pri
		left join sys.database_permissions perm  on pri.principal_id=perm.grantee_principal_id and perm.permission_name=''CONNECT'' and perm.class=0  /*DATABASE CONNECT*/
		where pri.[type] in (''S'',''U'',''G'') and pri.[sid] is not null 

		UNION ALL 

		/*DB ROLE MEMBERS*/
		select 
			case when isnull(suser_sname(dp2.[sid]),dp2.name)= dp2.name then dp2.name else suser_sname(dp2.[sid])+'' ''+quotename(dp2.name) end
			,dp2.type_desc
			,case when isnull(perm.permission_name,''no'')=''no'' then ''disabled'' else ''enabled'' end [status]
			,dp1.name [role]
		from sys.database_role_members drm
		join sys.database_principals dp1 on drm.role_principal_id=dp1.principal_id and dp1.type=''R''  /*Roles*/
		join sys.database_principals dp2 on drm.member_principal_id=dp2.principal_id and dp2.type&lt;&gt;''R''  /*Users*/
		left join sys.database_permissions perm  on dp2.principal_id=perm.grantee_principal_id and perm.permission_name=''CONNECT'' and perm.class=0 /*DATABASE CONNECT*/
)
, total as (
	select 
		db_name() [database]
		,[user]
		,[status]
		,[type_desc]
		,[public]
		,[db_owner]
		,[db_accessadmin]
		,[db_backupoperator]
		,[db_datareader]
		,[db_datawriter]
		,[db_ddladmin]
		,[db_denydatareader]
		,[db_denydatawriter]
		,[db_executor]
		,[db_securityadmin]
	from u 
	pivot 
	(count([role]) for [role] in ([public],[db_owner],[db_accessadmin],[db_backupoperator],[db_datareader],[db_datawriter]
	,[db_ddladmin],[db_denydatareader],[db_denydatawriter],[db_executor],[db_securityadmin],[others])) p
)
,extra as (/*db permissions per user */
		select 
			dp.class
			,dp.class_desc
			,case 
				when dp.class=0 /*DATABASE*/ then db_name() 
				when (dp.class=1 /*OBJECT_OR_COLUMN*/ and dp.major_id&gt;0) then OBJECT_NAME(dp.major_id) 
				when dp.class=3 /*SCHEMA*/ then SCHEMA_NAME(dp.major_id)
				else '''' end [Object]
			,dp.permission_name
			,dp.state_desc
			,us.name [user]
		from sys.database_permissions dp
		join sys.database_principals us 
					on  dp.grantee_principal_id=us.principal_id 
					and us.[type] in (''S'',''U'',''G'') 
					and us.[sid] is not null 
		where dp.[type]&lt;&gt;''CO'' /*connect*/
		and dp.class&lt;&gt;1
)
select 
	[database]
	,[user]
	,[status]
	,[type_desc]
	,[public]
	,[db_owner]
	,[db_accessadmin]
	,[db_backupoperator]
	,[db_datareader]
	,[db_datawriter]
	,[db_ddladmin]
	,[db_denydatareader]
	,[db_denydatawriter]
	,[db_executor]
	,[db_securityadmin]
	,isnull(stuff((select '', ''+[role] 
					from u u2 
					where u2.[user]=total.[user] 
					and u2.[role] not in (''public'',''db_owner'',''db_accessadmin'',''db_securityadmin'',''db_ddladmin'',''db_backupoperator''
								,''db_datareader'',''db_datawriter'',''db_denydatareader'',''db_denydatawriter'',''db_executor'')
				for XML PATH('''')),1,1,''''),'''') [other roles]
	,isnull(stuff((select distinct '', ''+[permission_name]+QUOTENAME(state_desc) 
					from extra u2 
					where u2.[user]=total.[user] 
				for XML PATH('''')),1,1,''''),'''') [other permissions]
from total 
option(recompile);'

/*
if @check_db &lt;&gt; '*** All Databases ***' 
	insert into @dbs(name) 
	values ( @check_db)
	
else*/
	insert into @dbs(name,compat)
	select name,compatibility_level
	from sys.databases 
	where name in (@check_db)
--	where databasepropertyex(name,'Status')='ONLINE' 
--	and DATABASEPROPERTYEX(name,'Collation') is not null 

while exists (select top 1 1 from @dbs)
	begin
		select top 1 @db=name,@compat=compat from @dbs 
		
		if @compat&gt;80
			begin
				select @tmp=replace(@cmd,'[placeholder]',quotename(@db))
				insert into #perms([database],[user],[status],[type_desc],[public],[db_owner],[db_accessadmin],[db_backupoperator],[db_datareader],[db_datawriter],[db_ddladmin],[db_denydatareader],[db_denydatawriter],[db_executor],[db_securityadmin],[other_roles],[other_permissions])
				exec(@tmp)
			end
		else 
			insert into #perms([database],[user],[status],[type_desc])
			select @db,'[Low Compatibility Level ('+convert(varchar(10),@compat)+'); check it manually, pls]','',''

		delete @dbs 
		where name=@db

		select @tmp=''
		select @db=''
		select @compat=0
	end

;with srv_roles as 
(
	select 
		l.name [Login]
		,g.name [Role]
		--,l.is_disabled
	from sys.server_role_members rm
	join sys.server_principals l on rm.member_principal_id=l.principal_id
	join sys.server_principals g on rm.role_principal_id=g.principal_id
)
select [database],[user],[status],[type_desc],[public],[db_owner],[db_accessadmin],[db_backupoperator],[db_datareader],[db_datawriter],[db_ddladmin],[db_denydatareader],[db_denydatawriter],[db_executor],[db_securityadmin],[other_roles],[other_permissions],
	isnull(stuff((select ', '+[Role] 
					from srv_roles u2 
					where u2.[Login]=replace(#perms.[user],' [dbo]','')
				for XML PATH('')),1,1,''),'') [server roles]
from #perms 

drop table #perms </CommandText>
      </Query>
