          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @Ver2 varchar(20)
declare @cmd varchar(2048)
--declare @db varchar(255)
declare @backup char(1)

--select @db='master'
select @backup='D'
select @Ver2=convert(varchar(20),serverproperty('ProductVersion'))
select @Ver2=left(@Ver2, charindex('.',@Ver2)-1)
select @cmd='
 select top 10 
		row_number() over (order by backup_set_id desc) [N]
		,bs.database_name
		, bs.recovery_model
		, convert(numeric(15,2),backup_size/1024/1024) [BackupSize_Mb]
		, ''no_compression'' [Compressed_MB]
		, convert(varchar(20),bs.backup_start_date,106)+'' ''+convert(varchar(20),bs.backup_start_date,108) [BackupStarted]
		, Case When datediff(ss,bs.backup_start_date,bs.backup_finish_date) &gt;= 86400
					Then Convert(VarChar(5), datediff(ss,bs.backup_start_date,bs.backup_finish_date)/86400) 
					+ '' days '' 
					Else ''''
					End
		   + Convert(VarChar(8), DateAdd(Second, datediff(ss,bs.backup_start_date,bs.backup_finish_date), 0), 108) as [Duration]
		, bs.user_name
		, case bs.type
			when ''D'' then ''Database full''
			when ''I'' then ''Database Differential''
			when ''L'' then ''Log''
			when ''F'' then ''File or Filegroup''
			when ''G'' then ''File Differential''
			when ''P'' then ''Partial''
			when ''Q'' then ''Partial Differential''
			else ''Unknown'' end ''backup type''  -- from BOL
		, bmf.physical_device_name
 from msdb.dbo.backupset bs
   inner join msdb..backupmediafamily bmf on bs.media_set_id=bmf.media_set_id
 where 
	bs.type='''+@backup+''' and bs.database_name=''' + @db+''' 
order by backup_set_id desc option (recompile)'

if convert(tinyint,@Ver2)&gt;=10
select @cmd=replace(@cmd,'''no_compression''','convert(numeric(15,2),compressed_backup_size/1024/1024)')

--select @cmd 
exec (@cmd)</CommandText>
      </Query>
