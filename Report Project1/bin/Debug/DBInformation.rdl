    <DataSet Name="DBObjects">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
declare @cmd varchar(4096)
select @cmd='use '+quotename(@db)+';'+char(10)
select @cmd=@cmd+'
select 
     type_desc [Object]
    ,count(1) [Amount]
from sys.objects 
where is_ms_shipped&lt;&gt;1
and type_desc&lt;&gt;''USER_TABLE''
group by type_desc 

union all 

select 
 case when type_desc=''NONCLUSTERED'' then ''NONCLUSTERED INDEX'' else ''USER_TABLE (''+ type_desc+'')'' end,
count(1)
from sys.indexes
where object_id not in (select object_id from sys.objects where is_ms_shipped=1 )--or type_desc not in (''SYSTEM_TABLE'',''INTERNAL_TABLE''))
group by case when type_desc=''NONCLUSTERED'' then ''NONCLUSTERED INDEX'' else ''USER_TABLE (''+ type_desc+'')'' end
order by 1 
option (recompile,maxdop 1)'
exec (@cmd)</CommandText>
      </Query>
      <Fields>
        <Field Name="Object">
          <DataField>Object</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
