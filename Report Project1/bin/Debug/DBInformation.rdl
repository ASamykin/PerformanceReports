    <DataSet Name="TopObjsBySize">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd varchar(4096)
select @cmd= 'use ['+@db+'];
SELECT top 15 
	0 [N]
	, ss.name [schema]
	, sab.name AS [object]
	, (sddps.reserved + ISNULL(sddit.reserved,0))/128 AS [SizeMB]
	, sddps.rows as [Records]
	, sddps.data/128 AS [DataMB]
	, (CASE WHEN (sddps.used + ISNULL(sddit.used,0)) &gt; sddps.data THEN (sddps.used + ISNULL(sddit.used,0)) - sddps.data ELSE 0 END) * 8 AS [IndexesKB]
	, (CASE WHEN (sddps.reserved + ISNULL(sddit.reserved,0)) &gt; sddps.used THEN (sddps.reserved + ISNULL(sddit.reserved,0)) - sddps.used ELSE 0 END) * 8 AS [UnusedKB]
FROM
	(SELECT 
		ps.object_id,
		SUM (
			CASE
				WHEN (ps.index_id &lt; 2) THEN row_count
				ELSE 0
			END
			) AS [rows],
		SUM (ps.reserved_page_count) AS reserved,
		SUM (
			CASE
				WHEN (ps.index_id &lt; 2) THEN (ps.in_row_data_page_count + ps.lob_used_page_count + ps.row_overflow_used_page_count)
				ELSE (ps.lob_used_page_count + ps.row_overflow_used_page_count)
			END
			) AS data,
		SUM (ps.used_page_count) AS used
	FROM sys.dm_db_partition_stats ps
	GROUP BY ps.object_id) AS sddps
LEFT OUTER JOIN 
	(SELECT 
		it.parent_id,
		SUM(ps.reserved_page_count) AS reserved,
		SUM(ps.used_page_count) AS used
	 FROM sys.dm_db_partition_stats ps
	 INNER JOIN sys.internal_tables it ON (it.object_id = ps.object_id)
	 WHERE it.internal_type IN (202,204)
	 GROUP BY it.parent_id) AS sddit ON (sddit.parent_id = sddps.object_id)
INNER JOIN sys.all_objects sab  ON ( sddps.object_id = sab.object_id ) 
INNER JOIN sys.schemas ss ON (sab.schema_id = ss.schema_id)
WHERE sab.type &lt;&gt; ''S'' and sab.type &lt;&gt; ''IT''
order by (sddps.reserved + ISNULL(sddit.reserved,0))/128 desc;'
exec (@cmd)
</CommandText>
      </Query>
      <Fields>
        <Field Name="N">
          <DataField>N</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="schema">
          <DataField>schema</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="object">
          <DataField>object</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SizeMB">
          <DataField>SizeMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
