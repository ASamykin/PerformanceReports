    <DataSet Name="VLFs">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd varchar(512)
select @cmd='
create table ##dbcc_loginfo  (
	RecoveryUnitID int,
	FileID smallint,
	FileSize bigint,
	StartOffset bigint,
	FSeqNo int,
	Status tinyint,
	Parity tinyint,
	CreateLSN varchar(50))'

if convert(tinyint,left(convert(varchar(20),serverproperty('ProductVersion')),charindex('.',convert(varchar(20),serverproperty('ProductVersion')))-1))&lt;11
	select @cmd=replace(@cmd,'RecoveryUnitID int,','')
if object_id('tempdb..##dbcc_loginfo')  is not null drop table ##dbcc_loginfo
exec (@cmd)

insert into ##dbcc_loginfo
exec ('dbcc loginfo('''+@db+''')')

select 
	count(1) [NumOfVLF]
	, convert(numeric(10,2),avg(convert(numeric(20,2),FileSize))/1024/1024) [AvgSizeMB]
	, convert(numeric(10,2),min(convert(numeric(20,0),FileSize))/1024/1024) [MinSizeMB]
	, convert(numeric(10,2),max(convert(numeric(20,0),FileSize))/1024/1024) [MaxSizeMB]
	, sum(convert(numeric(20,0),case when Status&lt;&gt;0 then FileSize/1024/1024 else 0 end)) [UsedSizeMB]
	, sum(case when Status&lt;&gt;0 then 1 else 0 end) [UsedVLFs]
from ##dbcc_loginfo

drop table ##dbcc_loginfo</CommandText>
