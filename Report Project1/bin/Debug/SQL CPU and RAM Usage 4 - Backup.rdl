    <DataSet Name="ListOfServers">
      <Query>
        <DataSourceName>SCOMDWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@srv">
            <Value>=Parameters!GuessServer.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct (DisplayName) [HostName]
	from OperationsManagerDW.dbo.vManagedEntity (NOLOCK)
where 
	Path like '%'+@srv+'%' 
	and ManagedEntityTypeRowId=100  /*Microsoft.SystemCenter.HealthService:FQDN*/
order by DisplayName
option (maxdop 1, recompile)</CommandText>
