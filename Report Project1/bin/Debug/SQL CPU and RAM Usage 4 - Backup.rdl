  <DataSets>
    <DataSet Name="SCOM_CPU_Raw">
      <Query>
        <DataSourceName>SCOMDWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt1">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dt2">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Servers">
            <Value>=Parameters!Servers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
              [ServerName] = left(me.Path,100)
              ,[DateTime]  = convert(datetime,ph.DateTime) 
              ,[Value]     = convert(numeric(5,2),avg(ph.SampleValue)) 
FROM OperationsManagerDW.dbo.vManagedEntity me
              join OperationsManagerDW.Perf.vPerfRaw ph on ph.ManagedEntityRowId = me.ManagedEntityRowId
              join OperationsManagerDW.dbo.vPerformanceRuleInstance pri on ph.PerformanceRuleInstanceRowId=pri.PerformanceRuleInstanceRowId
              join OperationsManagerDW.dbo.vPerformanceRule pr on pr.RuleRowId=pri.RuleRowId
WHERE 
	pr.ObjectName like '%Processor%'  --updated part with “like” to cater for different OS versions
	AND pr.CounterName = '% Processor Time'
	AND ph.DateTime &gt;= isnull(@dt1,dateadd(day,-1,getdate()))
	AND ph.DateTime &lt; isnull(@dt2,getdate())
	/*AND me.[Path]  like @srv+'%' */
	AND me.[Path] in (@Servers)
group by 
		me.Path
    , ph.DateTime
ORDER BY 
		[ServerName]
	, [DateTime]
option (maxdop 1, recompile)</CommandText>
        <Timeout>600</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateTime">
          <DataField>DateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SCOM_RAM_Raw">
      <Query>
        <DataSourceName>SCOMDWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt1">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dt2">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Servers">
            <Value>=Parameters!Servers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
              [ServerName] = left(me.Path,100) 
              ,[DateTime]  = convert(datetime,ph.DateTime) 
              ,[Value]     = convert(numeric(10,2),avg(ph.SampleValue)) 
FROM OperationsManagerDW.dbo.vManagedEntity me
              join OperationsManagerDW.Perf.vPerfRaw ph on ph.ManagedEntityRowId = me.ManagedEntityRowId
              join OperationsManagerDW.dbo.vPerformanceRuleInstance pri on ph.PerformanceRuleInstanceRowId=pri.PerformanceRuleInstanceRowId
              join OperationsManagerDW.dbo.vPerformanceRule pr on pr.RuleRowId=pri.RuleRowId
WHERE 
	pr.ObjectName = 'Memory'
	AND pr.CounterName in ('PercentMemoryUsed') --= 'RAM Free (%)'
	AND ph.DateTime &gt;= isnull(@dt1,dateadd(day,-1,getdate()))
	and ph.DateTime &lt; isnull(@dt2,getdate())
	/*and me.Path like @srv+'%'*/
	and me.Path in (@Servers)
group by me.Path
       , ph.DateTime
ORDER BY [DateTime]
option (maxdop 1, recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateTime">
          <DataField>DateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SCOM_CPU_Hours">
      <Query>
        <DataSourceName>SCOMDWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt1">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dt2">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Servers">
            <Value>=Parameters!Servers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        [ServerName]		   = left(me.Path,100) 
              ,[DateTime]		   = convert(datetime,ph.DateTime) 
			  ,[AverageValue]	   = ph.AverageValue - ph.MinValue
			  ,[MinValue]		   = ph.MinValue
			  ,[MaxValue]		   = ph.MaxValue-ph.AverageValue
			  ,[StandardDeviation] = ph.StandardDeviation
FROM OperationsManagerDW.dbo.vManagedEntity me
              join OperationsManagerDW.Perf.vPerfHourly ph on ph.ManagedEntityRowId = me.ManagedEntityRowId
              join OperationsManagerDW.dbo.vPerformanceRuleInstance pri on ph.PerformanceRuleInstanceRowId=pri.PerformanceRuleInstanceRowId
              join OperationsManagerDW.dbo.vPerformanceRule pr on pr.RuleRowId=pri.RuleRowId
WHERE 
	pr.ObjectName like '%Processor%'  --updated part with “like” to cater for different OS versions
	AND pr.CounterName = '% Processor Time'
	AND ph.DateTime &gt;= isnull(@dt1,dateadd(day,-1,getdate()))
	AND ph.DateTime &lt; isnull(@dt2,getdate())
	/*AND me.[Path]  like @srv+'%' */
	AND me.[Path]  in (@Servers)
option (maxdop 1, recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
