/* Section:  CIM_OperatingSystem */
set @sql='powershell.exe  -c "Get-CimInstance Win32_OperatingSystem | select Caption,Version,OSArchitecture,InstallDate,LastBootUpTime,LocalDateTime,FreePhysicalMemory,FreeVirtualMemory,FreeSpaceInPagingFiles,ForegroundApplicationBoost"' 

insert into @t (txt)
EXEC xp_cmdshell @sql

update @t 
set txt='a='+replace(txt,' :','|b=')+'|'

;with a as
(select cast('&lt;row '+replace(replace(txt, '=', '="'), '|', '" ')+' /&gt;' as xml) as xml_txt from @t),
b as (
	select 
		ltrim(rtrim(t.c.value('@a', 'varchar(200)'))) as [Param]
		,ltrim(rtrim(t.c.value('@b', 'varchar(200)'))) as [Value]
	from a
	cross apply xml_txt.nodes('/row') as t(c)
)
insert into @output(section,[param],[value])
select 

	case [Param]
		when 'FreePhysicalMemory' then 'RAM'
		when 'FreeVirtualMemory' then 'RAM'
		when 'FreeSpaceInPagingFiles' then 'RAM'
		else 'OS'
	end [section]
	,case [Param]
		when 'Caption' then 'Name'
		when 'OSArchitecture' then 'Architecture'
		else [Param]
	end [Verbose]
	,case 
		when ([Param] = 'ForegroundApplicationBoost' and [Value]='0') then 'None'
		when ([Param] = 'ForegroundApplicationBoost' and [Value]='1') then 'Minimum'
		when ([Param] = 'ForegroundApplicationBoost' and [Value]='2') then 'Maximum'
		else [Value]
	end [Value]
from b 

