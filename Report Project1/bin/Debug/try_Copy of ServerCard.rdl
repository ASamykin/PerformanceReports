declare @id int
declare @config tinyint
declare @sql varchar(400)
declare @t table (txt varchar(8000))

select @config=convert(tinyint,value_in_use) from sys.configurations where name='xp_cmdshell'

/*artificial delay (&lt;1s) to avoid parallel execution*/
declare @delay varchar(20)='00:00:00:'+right(convert(varchar(10),10000+convert(smallint,RAND()*1000)),3)
waitfor delay @delay 

/* sychronization of datasets */
if  object_id('tempdb.dbo.##performance_reports')  is null
	create table ##performance_reports (id int identity, username varchar(50),dt datetime, report varchar(50),dataset varchar(50),is_started tinyint,is_finished tinyint default 0, reconfig tinyint)

insert into ##performance_reports (username,dt,report,dataset,is_started,reconfig)
select @user,getdate(),@report,'dsSQLServices',1,@config

select @id=SCOPE_IDENTITY()
/*end of block*/


if @config=0
	begin
		if (select convert(tinyint,value_in_use) from sys.configurations where name='show advanced options')=0
			exec('exec sp_configure ''show advanced options'',1; reconfigure with override;' ) 
		exec('exec sp_configure ''xp_cmdshell'',1; reconfigure with override;' ) 
	end

set @sql='powershell.exe  -c "Get-WmiObject  win32_service  | where {$_.displayname -like ''SQL*''} | select name,state,startname,startmode,displayname,description | foreach{''a=''+$_.name+''|b=''+$_.state+''|c=''+$_.startname+''|d=''+$_.startmode+''|e=''+$_.displayname+''|''}"' 

insert into @t (txt)
EXEC xp_cmdshell @sql

/* log execution of the command  */
update ##performance_reports
set is_finished=1
where id=@id

;with a as
(select cast('&lt;row '+replace(replace(txt, '=', '="'), '|', '" ')+' /&gt;' as xml) as xml_txt from @t)

select 
	t.c.value('@a', 'varchar(100)') as [ServiceName]
	,t.c.value('@b', 'varchar(30)') as [State]
	,t.c.value('@c', 'varchar(100)') as [ServiceAccount]
	,t.c.value('@d', 'varchar(20)') as [StartMode]
	,t.c.value('@e', 'varchar(200)') as [DisplayName]
from a
cross apply xml_txt.nodes('/row') as t(c)
order by 2


if not exists (/*NOT all datasets are done*/select 1 from ##performance_reports where username=@user and report=@report and is_finished=0)
	and 
		exists (/*some of datasets have reconfigured the setting*/select 1 from ##performance_reports where username=@user and report=@report and reconfig=0)
		-- reconfigure it back
		exec('exec sp_configure ''xp_cmdshell'',0; reconfigure with override;' ) </CommandText>
      </Query>
      <Fields>
        <Field Name="ServiceName">
          <DataField>ServiceName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ServiceAccount">
          <DataField>ServiceAccount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="StartMode">
          <DataField>StartMode</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
