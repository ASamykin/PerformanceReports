	begin
		/*No GUI*/
		insert into @output(section,[param],[value])
		select 'OS','Is CORE Edition','Yes'

--		set @sql='powercfg.exe /L | find "*" | find /V "* Active"'
		set @sql='powercfg.exe /getactivescheme'
		delete from @t

		insert into @t (txt)
		EXEC xp_cmdshell @sql

		/*Power Scheme GUID: 8c5e7fda-e8bf-4a96-9a85-a6e23a8c635c  (High performance) **/

		update @t
			set txt='a='+substring(txt,charindex('(',txt)+1,charindex(')',txt)-charindex('(',txt)-1)+'|b=|'

	end
else 
	begin
		insert into @output(section,[param],[value])
		select 'OS','Is CORE Edition','no'

		set @sql='powershell.exe  -c "Get-WmiObject -Namespace root\cimv2\power -Class win32_PowerPlan | where {$_.IsActive -eq ''True''} | select ElementName,Description | foreach {''a=''+$_.ElementName+''|b=''+$_.Description+''|''}"' 

		delete from @t 

		insert into @t (txt)
		EXEC xp_cmdshell @sql

	end

