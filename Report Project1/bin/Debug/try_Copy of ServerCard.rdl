        <CommandText>select 
	i.[Principal Name]
	,i.[Connection String]
	,i.[Edition]
	,i.[Version]
	,i.[Version Name]
	,i.[Service Pack Version]
	,id.[Functional Owner]
	,id.[Instance Description]
