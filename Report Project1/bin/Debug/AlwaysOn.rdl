    <DataSet Name="dsListeners">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (
select 
	ag.name [agname]
	,al.dns_name [listenerdnsname]
	,al.port
	,ali.state_desc [status]
	,al.dns_name+':'+convert(varchar(10),al.port)+' ('+ali.state_desc+')' [verbose]
from sys.availability_group_listeners al
join sys.availability_groups ag on al.group_id=ag.group_id
join sys.availability_group_listener_ip_addresses ali on ali.listener_id=al.listener_id
)
select 
	d1.agname
,(select [verbose]+char(10) as 'data()' from data d2 where d2.agname=d1.agname for xml path('')) [listeners]
from data d1
option (recompile)
</CommandText>
      </Query>
      <Fields>
        <Field Name="agname">
          <DataField>agname</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="listeners">
          <DataField>listeners</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
