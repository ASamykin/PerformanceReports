    <DataSet Name="dsAvailabilityGroups">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
select distinct
ag.name
	,case 
		when ag.failure_condition_level = 1 then '[1] When SQL Server service is down.'
		when ag.failure_condition_level = 2 then '[2] The instance of SQL Server does not connect to cluster and HEALTH_CHECK_TIMEOUT threshold is exceeded.'+char(10)+'The availability replica is in failed state.'
		when ag.failure_condition_level = 3 then '[3] On critical SQL Server internal errors, such as orphaned spinlocks, serious write-access violations or too much dumping.'
		when ag.failure_condition_level = 4 then '[4] On moderate SQL Server internal errors, such as a persistent out-of-memory condition in the SQL Server internal resource pool.'
		when ag.failure_condition_level = 5 then '[5] On any qualified failure conditions, including exhaustion of SQL Engine worker-threads, detection of an unsolvable deadlock.'
	end [AutomaticFailoverCondition]
	,ag.health_check_timeout/1000 [timeout_s]
	,ag.automated_backup_preference_desc
	,isnull(ar.replica_server_name,cn.replica_server_name) [replica_server_name]
	,ar.availability_mode_desc +char(10)+ar.failover_mode_desc + ' failover' [AvailabilityFailover]
	,ars.is_local
	,case when ag.name is not null and ars.role_desc is null then '[not known]' else ars.role_desc end [role_desc]
	,ars.operational_state
	,ars.operational_state_desc
	,ars.connected_state
	,ars.connected_state_desc
	,ars.recovery_health
	,ars.recovery_health_desc
	,ars.synchronization_health
	,ars.synchronization_health_desc
	,ars.last_connect_error_timestamp
	,ars.last_connect_error_number
	,ars.last_connect_error_description
,(select count(1) from sys.dm_hadr_database_replica_states rs2 where rs2.group_id=ag.group_id and rs2.is_local=1) [DBs]
,(select count(1) from sys.availability_group_listeners where group_id=ag.group_id) [Listeners]
from sys.availability_groups ag
join sys.availability_replicas ar on ag.group_id=ar.group_id
left join sys.dm_hadr_availability_replica_states ars on ars.replica_id=ar.replica_id
right join sys.dm_hadr_availability_replica_cluster_nodes cn on ar.replica_server_name=cn.replica_server_name</CommandText>
      </Query>
