    <DataSet Name="dsARStateChange">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

DECLARE @FileName NVARCHAR(4000)
declare @events table 
(dt datetime,prev_state varchar(50),curr_state varchar(50),ar_name varchar(50), ag_name varchar(50))

if exists (select 1 from sys.dm_xe_sessions where name = 'AlwaysOn_health')
	begin
		SELECT @FileName = target_data.value('(EventFileTarget/File/@name)[1]','nvarchar(4000)')
		FROM (
				SELECT
					CAST(target_data AS XML) target_data
					FROM sys.dm_xe_sessions s
					JOIN sys.dm_xe_session_targets t
						ON s.address = t.event_session_address
					WHERE s.name = N'AlwaysOn_health'
			) ft
		insert into @events(dt,prev_state,curr_state,ar_name,ag_name)
		SELECT 
			XEData.value('(event/@timestamp)[1]','datetime2(3)') AS event_timestamp,
			XEData.value('(event/data[@name="previous_state"]/text)[1]', 'varchar(255)') AS previous_state,
			XEData.value('(event/data[@name="current_state"]/text)[1]', 'varchar(255)') AS current_state,
			XEData.value('(event/data[@name="availability_replica_name"]/value)[1]', 'varchar(255)') AS availability_replica_name,
			XEData.value('(event/data[@name="availability_group_name"]/value)[1]', 'varchar(255)') AS availability_group_name
		FROM (
				SELECT CAST(event_data AS XML) XEData, *
				FROM sys.fn_xe_file_target_read_file(@FileName, NULL, NULL, NULL)
				WHERE object_name = 'availability_replica_state_change'
			 ) event_data
		--WHERE 
		--XEData.value('(event/data[@name="current_state"]/text)[1]', 'varchar(255)') in  ('PRIMARY_NORMAL','SECONDARY_NORMAL')
		--and XEData.value('(event/@timestamp)[1]','datetime2(3)')&gt;=convert(date,getdate())
		--ORDER BY event_timestamp DESC;
end

select dt [dt]
	,'&lt;= '+curr_state [state]
	,ar_name [arname]
	,ag_name [agname]
	--,sum(case when dt&gt;=convert(date,getdate()) then 1 else 0 end) [issuestoday]
from @events
--group by dt 
--	,'&lt;= '+curr_state 
--	,ar_name
--	,ag_name

union all 

select 
	getdate() 
	,case 
		when gs.primary_recovery_health is not null then 'PRIMARY'  
		else 'SECONDARY' 
	end 
	+char(10)+
	lower(gs.synchronization_health_desc) [state]
	,gs.primary_replica
	,ag.name
--	,0
from sys.dm_hadr_availability_group_states  gs
join sys.availability_groups ag on ag.group_id=gs.group_id

order by dt desc 


</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="state">
          <DataField>state</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="arname">
          <DataField>arname</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="agname">
          <DataField>agname</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
