    <DataSet Name="CPUUsage">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
declare @cmd varchar(2048)
if object_id('tempdb..CPUUsageHistory_11662233') is not null 
	select @cmd='
		select 	
			convert(varchar(10),dt,108) [DateTime]
			, SQLUtil [SQL]
			, 100-SQLUtil-SystemIdle [OS]
	from tempdb..CPUUsageHistory_11662233 (NOLOCK)
	where dt &gt;=convert(varchar(10),getdate(),112)
	order by dt'
else 
	select @cmd='SELECT DATEADD (ms, a.[Record Time] - sys.ms_ticks, GETDATE()) AS [DateTime],    
              a.[ProcessUtilization] [SQL],
              100-a.[SystemIdle %]-a.[ProcessUtilization] [OS] 
FROM   (SELECT x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/ProcessUtilization)[1]'', ''int'') AS [ProcessUtilization],    
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/SystemIdle)[1]'', ''int'') AS [SystemIdle %],   
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/UserModeTime) [1]'', ''bigint'') AS [UserModeTime],   
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/KernelModeTime) [1]'', ''bigint'') AS [KernelModeTime],    
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/PageFaults) [1]'', ''bigint'') AS [PageFaults],   
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/WorkingSetDelta) [1]'', ''bigint'')/1024 AS [WorkingSetDelta],   
                     x.value(''(//Record/SchedulerMonitorEvent/SystemHealth/MemoryUtilization) [1]'', ''bigint'') AS [MemoryUtilization (%workingset)],   
                     x.value(''(//Record/@time)[1]'', ''bigint'') AS [Record Time]  
       FROM (SELECT CAST (record as xml) 
                     FROM sys.dm_os_ring_buffers    
                     WHERE ring_buffer_type = ''RING_BUFFER_SCHEDULER_MONITOR'') AS R(x)) a  
              CROSS JOIN sys.dm_os_sys_info sys'
exec (@cmd)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DateTime">
          <DataField>DateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SQL">
          <DataField>SQL</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OS">
          <DataField>OS</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ServerInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>declare @cmd varchar(2048)
select @cmd='
select 
	@@servername [SQLServer]
	, SERVERPROPERTY(''ComputerNamePhysicalNetBIOS'') [HostName]
	, convert(varchar(20), sqlserver_start_time,106)+ '' '' +convert(varchar(10), 	sqlserver_start_time,108) [Started]
	, Case When datediff(ss,sqlserver_start_time,getdate()) &gt;= 86400  
		     Then Convert(VarChar(5), datediff(ss,sqlserver_start_time,getdate())/86400) + '' days ''   
     Else '''' End  
	     + Convert(VarChar(8), DateAdd(Second, datediff(ss,sqlserver_start_time,getdate()), 0), 108) [SQLUptime]'+
	case 
		when convert(varchar(20),SERVERPROPERTY('ProductVersion')) like '10.0.%' then ',''see @@version'' [ServerType]' 
		else 
	', case 
		when virtual_machine_type_desc=''NONE'' then ''Physical''
		when virtual_machine_type_desc=''HYPERVISOR'' then ''Virtual'' 
		else virtual_machine_type_desc end [ServerType]' end 
	+
	', getdate() [ServerTime]
	, replace(replace(replace(@@version,char(10),''''),char(13),''''),''Corporation'', ''Corporation'') [SQLVersion]
	, (select convert(varchar(20),min(create_date),106) from sys.server_principals where name in (''RABONETEU\eu.mgt.DatabaseAdmins.ls'',''NT AUTHORITY\SYSTEM'')) [Installed]
from sys.dm_os_sys_info option (recompile)'
exec (@cmd)</CommandText>
      </Query>
