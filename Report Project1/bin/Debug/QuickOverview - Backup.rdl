    <DataSet Name="DisksInfo_2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
declare @drives table (drive varchar(5),MBFree int)
declare @drive_info table (
	volume_mount_point nvarchar(512),
	file_system_type nvarchar(512),
	logical_volume_name nvarchar(512),
	[Total_Size_GB] DECIMAL(18,2),
	[Available_Size_GB] DECIMAL(18,2),
	[Space_Free_Percent] DECIMAL(18,2))


insert into @drives (drive,MBFree)
exec xp_fixeddrives 

if convert(varchar(20),SERVERPROPERTY('ProductVersion')) not like '10.0.%' 
	begin
		declare @cmd varchar(2048)
		select @cmd=
					'with files(database_id,file_id) as
						(select 
							min(database_id)
							,min(file_id)
						from sys.master_files with (NOLOCK)
						group by left(physical_name,1),database_id)
						SELECT vs.volume_mount_point
						, vs.file_system_type
						, vs.logical_volume_name
						, CONVERT(DECIMAL(18,2),vs.total_bytes/1024/1024/1024.0) AS [Total_Size_GB]
						, CONVERT(DECIMAL(18,2),vs.available_bytes/1024/1024/1024.0) AS [Available_Size_GB]
						, CAST(CAST(vs.available_bytes AS FLOAT)/ CAST(vs.total_bytes AS FLOAT) AS DECIMAL(18,2)) * 100 AS [Space_Free_Percent] 
						FROM files AS f WITH (NOLOCK)
						CROSS APPLY sys.dm_os_volume_stats(f.database_id, f.[file_id]) AS vs 
						OPTION (RECOMPILE)'
		--select @cmd
		insert into @drive_info([volume_mount_point],[file_system_type],[logical_volume_name],[Total_Size_GB],[Available_Size_GB],[Space_Free_Percent])
		exec (@cmd)
	end 

;with t1 (d,f) as 
(
select 
	left(physical_name,1) [Drive],
		type_desc+' ('+convert(varchar(10),count(1))+')' 
from sys.master_files with (nolock)
group by left(physical_name,1),  type_desc
)
select  distinct
	isnull(di.[volume_mount_point],d.drive+':\') [volume_mount_point]
	,isnull(convert(varchar(20),di.[file_system_type]),'') [file_system_type]
	,isnull(convert(varchar(20),di.[logical_volume_name]),'') [logical_volume_name]
	,isnull(convert(varchar(20),di.[Total_Size_GB]),'') [Total_Size_GB]
	,isnull(convert(varchar(20),di.[Available_Size_GB]),convert(varchar(20),d.MBFree/1024)) [Available_Size_GB]
	,isnull(convert(varchar(20),di.[Space_Free_Percent]),'') [Space_Free_Percent]
	,isnull(reverse(stuff(reverse((select f + ', ' as 'data()' from t1 t2 where t2.d=t1.d for xml path('') )),1,2,'')),'') [Type&amp;Amount]
from @drives d
left join @drive_info di on left(di.volume_mount_point,1)=d.drive
left join t1 on t1.d=d.drive</CommandText>
      </Query>
      <Fields>
        <Field Name="volume_mount_point">
          <DataField>volume_mount_point</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="file_system_type">
          <DataField>file_system_type</DataField>
