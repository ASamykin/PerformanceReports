  <DataSets>
    <DataSet Name="dsServers">
      <Query>
        <DataSourceName>DBAInventory</DataSourceName>
        <CommandText>if object_id('tempdb.dbo.Report_CompareServers') is null 
	   create table tempdb.dbo.Report_CompareServers (dt datetime default getdate(), servername varchar(100))
/*
select distinct servername
from tempdb.dbo.Report_CompareServers
*/
SELECT [Instance] [servername]
  FROM [dbo].[raw_SQLServer]
  order by 1 </CommandText>
      </Query>
