    <DataSet Name="dsConfigurations">
      <Query>
        <DataSourceName>DBAInventory</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@show_diff">
            <Value>=iif(Parameters!show_diff.Value=True,1,0)</Value>
          </QueryParameter>
          <QueryParameter Name="@servers">
            <Value>=Parameters!Servers.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
set nocount on
if object_id('tempdb.dbo.#compare') is not null drop table #compare 
if object_id('tempdb.dbo.#configurations') is not null drop table #configurations

if object_id('tempdb.dbo.Report_CompareServers') is null 
   create table tempdb.dbo.Report_CompareServers (dt datetime default getdate(),servername varchar(100))

select servername
into #compare
from tempdb.dbo.Report_CompareServers
where servername in (@servers)

/*
-- for debug 
;with data as (select 'NLAMS00304' [servername] union all select 'NLAMS10304')
select servername 
into #compare 
from data 
*/

create table  #configurations  
(srv varchar(50) null,src varchar(50),configuration_id int,name nvarchar(35)
,value sql_variant,value_in_use sql_variant,description nvarchar(255),is_dynamic bit,is_advanced bit)

declare @srv varchar(50)
declare @cmd varchar(8000)
declare @addon varchar(100)
--declare @show_diff int = 1

while exists (select top 1 1 from #compare)
	begin
		select top 1 @srv=servername from #compare

		-- (re-)create linked server for data retrieval 
		exec usp_LinkedServer @srv,'SC_COMPARE_SERVERS'

--------------------------------------------------------  
		/* SYS.CONFIGURATIONS */
		select @cmd='if object_id(''tempdb.dbo.configurations'') is not null exec (''drop table tempdb.dbo.configurations''); create table tempdb.dbo.configurations (configuration_id int,name nvarchar(35),value sql_variant,value_in_use sql_variant,description nvarchar(255),is_dynamic bit,is_advanced bit);'+char(10)--+'go'+char(10)
		select @cmd=@cmd+'insert into tempdb.dbo.configurations (configuration_id,name,value,value_in_use,description,is_dynamic,is_advanced) select configuration_id,name,value,value_in_use,description,is_dynamic,is_advanced from sys.configurations;'

		exec (@cmd) at [SC_COMPARE_SERVERS]

		insert into #configurations(src,configuration_id,name,value,value_in_use,[description],is_dynamic,is_advanced)
		exec ('select ''sys.configurations'',configuration_id,name,value,value_in_use,description,is_dynamic,is_advanced from [SC_COMPARE_SERVERS].tempdb.dbo.configurations')

		exec('drop table tempdb.dbo.configurations') at [SC_COMPARE_SERVERS]

--------------------------------------------------------  
		/* prepare temp table for the next queries */
		select @cmd='if object_id(''tempdb.dbo.dmvs'') is not null exec (''drop table tempdb.dbo.dmvs''); create table tempdb.dbo.dmvs (id int identity,source varchar(50),param varchar(500),val varchar(500));'
		exec (@cmd) at [SC_COMPARE_SERVERS]

--------------------------------------------------------  
		/* Authentication and Login auditing */
		select @cmd=
		'
		declare @tbl table (name varchar(50),config_value varchar(50))
		insert into @tbl (name,config_value)
		exec xp_loginconfig


		insert 
			into tempdb.dbo.dmvs (source,param,val)
		select ''Authentication'', name, config_value
		from @tbl 
		where name in (''login mode'',''audit level'')
		'

		exec (@cmd) at [SC_COMPARE_SERVERS]

		insert into #configurations(src,configuration_id,name,value,value_in_use,[description],is_dynamic,is_advanced)
		exec ('select source,id,param,val,val,'''',0,0 from [SC_COMPARE_SERVERS].tempdb.dbo.dmvs')		 

--------------------------------------------------------  
		/* DMVs and SERVERPROPERTY() */
		select @cmd='with os_sys_memory as 
	(
	select 
		convert(varchar(100),total_physical_memory_kb) [total_physical_memory_kb]
		,convert(varchar(100),available_physical_memory_kb) [available_physical_memory_kb]
		,convert(varchar(100),total_page_file_kb) [total_page_file_kb]
		,convert(varchar(100),available_page_file_kb) [available_page_file_kb]
		,convert(varchar(100),system_memory_state_desc) [system_memory_state_desc]
	from sys.dm_os_sys_memory
	)
, os_windows_info as 
	(
	select 
		convert(varchar(100),windows_release) [windows_release]
		,convert(varchar(100),windows_service_pack_level) [windows_service_pack_level]
		,convert(varchar(100),windows_sku) [windows_sku]
		,convert(varchar(100),os_language_version) [os_language_version]
	from sys.dm_os_windows_info
	)
, os_sys_info as
	(
	select 
		convert(varchar(100),cpu_count) [cpu_count]
		,convert(varchar(100),hyperthread_ratio) [hyperthread_ratio]
		--,convert(varchar(100),physical_memory_kb) [physical_memory_kb]
		--,convert(varchar(100),virtual_memory_kb) [virtual_memory_kb]
		--,convert(varchar(100),committed_kb) [committed_kb]
		--,convert(varchar(100),committed_target_kb) [committed_target_kb]
		--,convert(varchar(100),visible_target_kb) [visible_target_kb]
		,convert(varchar(100),os_quantum) [os_quantum]
		,convert(varchar(100),os_error_mode) [os_error_mode]
		,convert(varchar(100),os_priority_class) [os_priority_class]
		,convert(varchar(100),max_workers_count) [max_workers_count]
		,convert(varchar(100),scheduler_count) [scheduler_count]
		,convert(varchar(100),scheduler_total_count) [scheduler_total_count]
		,convert(varchar(100),sqlserver_start_time) [sqlserver_start_time]
		,convert(varchar(100),affinity_type_desc) [affinity_type_desc]
		,convert(varchar(100),virtual_machine_type_desc) [virtual_machine_type_desc]
	from sys.dm_os_sys_info 
	)
, server_props as 
	(
		select convert(varchar(100),SERVERPROPERTY(''MachineName'')) [val], ''MachineName'' [param] union all 
		select convert(varchar(100),SERVERPROPERTY(''ComputerNamePhysicalNetBIOS'')) [val], ''ComputerNamePhysicalNetBIOS'' [param] union all 
		select convert(varchar(100),SERVERPROPERTY(''InstanceName'')) [val], ''InstanceName'' [param] union all -- DBA_PRD_01

		select convert(varchar(100),SERVERPROPERTY(''Edition''))   [val], ''Edition'' [param] union all  -- Standard Edition (64-bit)
		select convert(varchar(100),SERVERPROPERTY(''ProductVersion''))  [val], ''ProductVersion'' [param] union all 
		select convert(varchar(100),SERVERPROPERTY(''ProductLevel''))  [val], ''ProductLevel'' [param] union all -- SP3
		select convert(varchar(100),SERVERPROPERTY(''Collation'')) [val], ''Collation'' [param] union all -- SQL_Latin1_General_CP1_CI_AS
		select convert(varchar(100),case when SERVERPROPERTY(''IsIntegratedSecurityOnly'')=1 then ''Windows'' else ''Mixed'' end)  [val], ''SecurityMode'' [param] union all  -- 0

		select convert(varchar(100),SERVERPROPERTY(''IsClustered''))  [val], ''IsClustered'' [param] union all  -- 0
		select convert(varchar(100),SERVERPROPERTY(''InstanceDefaultDataPath''))  [val], ''DefaultDataPath'' [param] union all  -- F:\MSSQL$DBA_PRD_01\UserData\
		select convert(varchar(100),SERVERPROPERTY(''InstanceDefaultLogPath''))  [val], ''DefaultLogPath'' [param] union all -- G:\MSSQL$DBA_PRD_01\UserLog\


		select convert(varchar(100),SERVERPROPERTY(''ProductMajorVersion''))  [val], ''ProductMajorVersion'' [param] union all   -- 11
		select convert(varchar(100),SERVERPROPERTY(''ProductMinorVersion''))  [val], ''ProductMinorVersion'' [param] union all  -- 0
		select convert(varchar(100),SERVERPROPERTY(''ProductBuild''))  [val], ''ProductBuild'' [param] union all -- 6248
		select convert(varchar(100),SERVERPROPERTY(''ProductUpdateLevel''))  [val], ''ProductUpdateLevel'' [param]
	)

insert into tempdb.dbo.dmvs (source,param,val)

select 
	''sys.os_sys_memory'' [view]
	,param [param]
	,val
from os_sys_memory t
unpivot
(val  for param in ([total_physical_memory_kb],[available_physical_memory_kb],[total_page_file_kb],[available_page_file_kb],[system_memory_state_desc])
) as pvt
  UNION ALL 
select 
	''sys.os_windows_info'' [view]
	,param [param]
	,val
from os_windows_info t
unpivot
(val  for param in ([windows_release],[windows_service_pack_level],[windows_sku],[os_language_version])
) as pvt
  UNION ALL
select 
	''sys.os_sys_info'' [view]
	,param [param]
	,val
from os_sys_info t
unpivot
(val  for param in ([cpu_count],[hyperthread_ratio],/*[physical_memory_kb],[virtual_memory_kb],[committed_kb],[committed_target_kb],[visible_target_kb],*/
					[os_quantum],[os_error_mode],[os_priority_class],[max_workers_count],[scheduler_count],[scheduler_total_count],[sqlserver_start_time],[affinity_type_desc],[virtual_machine_type_desc])
) as pvt
  UNION ALL
select 
	''SERVERPROPERTY()'' [view]
	,param
	,val
from server_props'

		exec (@cmd) at [SC_COMPARE_SERVERS]

		insert into #configurations(src,configuration_id,name,value,value_in_use,[description],is_dynamic,is_advanced)
		exec ('select ''dmvs and serverproperty()'',id,param,val,val,'''',0,0 from [SC_COMPARE_SERVERS].tempdb.dbo.dmvs')

--------------------------------------------------------  
	/*disk drives*/
	select @cmd='truncate table tempdb.dbo.dmvs;
declare @free table (drive varchar(5),mb_free int)
insert into @free (drive,mb_free)
exec xp_fixeddrives

;with t1 as 
	(
	select 
		left(physical_name,1) [drive]
		,min(database_id) [dbid]
		,min(file_id) [fileid]
	from sys.master_files with (NOLOCK)
	group by 
		left(physical_name,1)
		,database_id
	)
,t2 as 
	(
	select 
		drive
		,min(dbid) [dbid]
		,fileid
	from t1 
	group by drive,fileid
	)
, t3 as (
	SELECT vs.volume_mount_point
		, vs.file_system_type
		, vs.logical_volume_name
		, CONVERT(DECIMAL(18,0),vs.total_bytes/1024/1024.0) AS [Total_Size_MB]
		, CONVERT(DECIMAL(18,0),vs.available_bytes/1024/1024.0) AS [Available_Size_MB]
		, CAST(CAST(vs.available_bytes AS FLOAT)/ CAST(vs.total_bytes AS FLOAT) AS DECIMAL(18,2)) * 100 AS [Space_Free_Percent] 
	FROM t2 AS f WITH (NOLOCK)
	CROSS APPLY sys.dm_os_volume_stats(f.dbid, f.[fileid]) AS vs)
insert into  tempdb.dbo.dmvs (source,param,val)
select 
	''Disk drives'' [source]
	,''Drive '' + quotename(left(isnull(t3.volume_mount_point,f.drive),1)) [param]
	,isnull(''File system: '' + t3.file_system_type +char(10),'''')+
	isnull(''Volume name: '' + t3.logical_volume_name +char(10),'''')+
	isnull(''Total (MB): ''  + convert(varchar(20),t3.Total_Size_MB) +char(10),'''')+
	''Free (MB):  ''  + isnull(convert(varchar(20),t3.Available_Size_MB),f.mb_free) [val] 
from t3 
right join @free f on t3.volume_mount_point=f.drive+'':\'''
	
		exec (@cmd) at [SC_COMPARE_SERVERS]

		insert into #configurations(src,configuration_id,name,value,value_in_use,[description],is_dynamic,is_advanced)
		exec ('select source,id,param,val,val,'''',0,0 from [SC_COMPARE_SERVERS].tempdb.dbo.dmvs')

--------------------------------------------------------  
	/*Databases and Logins*/
	select @cmd='truncate table tempdb.dbo.dmvs;
insert into  tempdb.dbo.dmvs (source,param,val)
select   ''Databases'' [source]
		,''Databases, Total'' [param]
		,convert(varchar(200),count(1))  [val] 
from sys.databases 
union all
select   ''Databases'' [source]
		,''DB Size (MB), Total'' [param]
		,convert(varchar(200),sum(size/128))  [val] 
from sys.master_files 
union all
select  ''Databases'' [source]
		,''Tempdb files, Total'' [param]
		,count(1)  [val] 
from sys.master_files 
where database_id=2

union all

/* for many databases this recordset is too big
select 
	''Databases'' [source]
	,''Database ''+quotename(d.name) [param]
	,
	''Owner: ''+isnull(convert(varchar(200),suser_sname(d.owner_sid)),''&lt;unknown&gt;'')+char(10)
	+''Status: ''+convert(varchar(200),DATABASEPROPERTYEX(d.name,''Status'')) +char(10)
	+''Recovery: ''+ convert(varchar(200),DATABASEPROPERTYEX(d.name,''Recovery'')) +char(10)
	+''Data Size (MB): ''+convert(varchar(20),sum(case when mf.type=0 /*ROWS*/ then size else 0 end)/128) + char(10)
	+''Log Size (MB): ''+convert(varchar(20),sum(case when mf.type=1 /*LOG*/ then size else 0 end)/128) + char(10)
	+''Files: ''+convert(varchar(20),count(mf.database_id)) [val]
from sys.master_files mf
join sys.databases d on mf.database_id=d.database_id 
group by 
	d.name
	,d.owner_sid

Union ALL
*/
select 
	''Logins'' [source]
	,''Logins, Total'' [param]
	,convert(varchar(200),count(1)) [val]
from sys.server_principals 
--where type in (''S'',''U'',''G'')
where type&lt;&gt;''R''
/*
union all
select 
	''Logins'' [source]
	,''Login ''+quotename(name) [param]
	,
	''Status: ''+case when is_disabled=1 then ''disabled'' else ''enabled'' end + char(10)
	+''Type: ''+convert(varchar(200),type_desc)+char(10)
	+''Default DB: ''+default_database_name+char(10)
	+''Language: ''+isnull(default_language_name,''&lt;not set&gt;'')
	+''Server roles: '' + isnull(stuff((select '', ''+g.name 
					from sys.server_role_members rm
						join sys.server_principals g on rm.role_principal_id=g.principal_id 
					where rm.member_principal_id=sp.principal_id
				for XML PATH('''')),1,1,''''),'''') 
	+''Server Permissions: '' + isnull(stuff((select '', ''+p.permission_name+'' ''+quotename(p.state)
					from sys.server_permissions p
					where p.grantee_principal_id=sp.principal_id
				for XML PATH('''')),1,1,''''),'''') 
	collate SQL_Latin1_General_CP1_CI_AS [val] 
from sys.server_principals  sp
--where type in (''S'',''U'',''G'')
where type&lt;&gt;''R''   */'
	
		exec (@cmd) at [SC_COMPARE_SERVERS]

		insert into #configurations(src,configuration_id,name,value,value_in_use,[description],is_dynamic,is_advanced)
		exec ('select source,id,param,val,val,'''',0,0 from [SC_COMPARE_SERVERS].tempdb.dbo.dmvs')



/*clean-up*/
		exec('drop table tempdb.dbo.dmvs') at [SC_COMPARE_SERVERS]

		update #configurations
		set srv=@srv 
		where srv is null

		delete #compare where servername=@srv
	end

select @addon=''
if @show_diff = 1 select @addon=' and c2.value_in_use&lt;&gt;c1.value_in_use'

select @cmd=' 
select  distinct
	c1.src
	,c1.srv [srv1]
	,c1.name
	,c1.value_in_use [val1]
	,c2.value_in_use [val2]
	,c2.srv [srv2]
	,c1.description
from #configurations c1
join #configurations c2 on /*c2.configuration_id=c1.configuration_id*/ c2.name=c1.name and c2.srv&lt;&gt;c1.srv 
where c1.srv='''+@srv+''' '+@addon

--print @cmd

exec (@cmd)</CommandText>
      </Query>
      <Fields>
        <Field Name="src">
          <DataField>src</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="srv1">
          <DataField>srv1</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="val1">
          <DataField>val1</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="val2">
          <DataField>val2</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="srv2">
          <DataField>srv2</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsInventory">
      <Query>
