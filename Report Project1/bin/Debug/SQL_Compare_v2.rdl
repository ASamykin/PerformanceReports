        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
set nocount on

declare @srv varchar(255) -- servername
declare @cmd1 varchar(8000)
declare @addon varchar(100)
--declare @show_diff int = 1

declare @q1 varchar(8000)  /*raw_ComputerSystemDetails*/
declare @q2 varchar(8000)  /*raw_OSDetails*/
declare @q3 varchar(8000)  /*raw_PowerPlan*/
declare @q4 varchar(8000)  /*raw_Processor*/
declare @q5 varchar(8000)  /*raw_SQLServices*/
declare @q6 varchar(8000)  /*raw_SQLServer*/

if object_id('tempdb.dbo.#compare') is not null drop table #compare 
if object_id('tempdb.dbo.#inventory') is not null drop table #inventory

create table #inventory (srv varchar(255) null,parameter varchar(50),val varchar(255))

if object_id('tempdb.dbo.Report_CompareServers') is null 
   create table tempdb.dbo.Report_CompareServers (dt datetime default getdate(), servername varchar(100),instanceID int)
/**/
select servername
into #compare
from tempdb.dbo.Report_CompareServers
where servername in (@servers)
/**/
