/*raw_ComputerSystemDetails*/
set @q1=
'select parameter, replace(val,''"'','''') [val]
from 
(select convert(varchar(255),[systemname]) [systemname]
	,convert(varchar(255),[domain]) [domain]
	,convert(varchar(255),[model]) [model]
	,convert(varchar(255),[logicalprocessors]) [logicalprocessors]
	,convert(varchar(255),[processors]) [processors]
	,convert(varchar(255),[totalram]) [totalram]
FROM [dbo].[raw_ComputerSystemDetails]
where [systemname] = ''@srv_name''
) t
unpivot
(val for [Parameter] in
	(
	  -- [systemname],
	  [domain]
      ,[model]
      ,[logicalprocessors]
      ,[processors]
      ,[totalram]
	  )
) a 
option (recompile)
'

/*raw_OSDetails*/
set @q2=
'
select parameter, replace(val,''"'','''') [val]
from 
(select 
	convert(varchar(255),[systemname]) [systemname]
	,convert(varchar(255),[caption]) [OScaption]
	,convert(varchar(255),[version]) [OSversion]
	,convert(varchar(255),[architecture]) [OSarchitecture]
	,convert(varchar(255),[installdate]) [OSinstalldate]
	,convert(varchar(255),[lastbootup]) [OSlastbootup]
	,convert(varchar(255),[localdatetime]) [OSlocaldatetime]
	,convert(varchar(255),[memoryphysicalfree]) [OSmemoryphysicalfree]
	,convert(varchar(255),[memoryvirtualfree]) [OSmemoryvirtualfree]
	,convert(varchar(255),[pagefilesfreespace]) [OSpagefilesfreespace]
	,convert(varchar(255),[foregroungappboost]) [OSforegroungappboost]
	,convert(varchar(255),[status]) [OSstatus]
FROM [dbo].[raw_OSDetails]
where [systemname] = ''@srv_name''
) t
unpivot
(val for [Parameter] in
	(
	   --[systemname],
      [OScaption]
      ,[OSversion]
      ,[OSarchitecture]
      ,[OSinstalldate]
      ,[OSlastbootup]
      ,[OSlocaldatetime]
      ,[OSmemoryphysicalfree]
      ,[OSmemoryvirtualfree]
      ,[OSpagefilesfreespace]
      ,[OSforegroungappboost]
      ,[OSstatus]
	  )
) a 
option (recompile)
'

/*raw_PowerPlan*/
set @q3=
'
select parameter, replace(val,''"'','''') [val]
from 
(select convert(varchar(255),[systemname]) [systemname]
	,convert(varchar(255),[currentplan]) [CurrentPowerPlan]
	,convert(varchar(255),[description]) [description]
FROM [dbo].[raw_PowerPlan]
where [systemname] = ''@srv_name''
) t
unpivot
(val for [Parameter] in
	(
	   --[systemname],
      [CurrentPowerPlan]
      ,[description]
	  )
) a 
option (recompile)
'

/*raw_Processor*/
set @q4=
'
select parameter, replace(val,''"'','''') [val]
from 
(select convert(varchar(255),[systemname]) [systemname]
	,convert(varchar(255),[name]) [CPUname]
	,convert(varchar(255),[cores]) [CPUcores]
	,convert(varchar(255),[logicalprocessors]) [CPUlogicalprocessors]
	,convert(varchar(255),[clockspeedmax]) [CPUclockspeedmax]
	,convert(varchar(255),[clockspeedcurrent]) [CPUclockspeedcurrent]
	,convert(varchar(255),isnull([l2cache],'''')) [CPUl2cache]
	,convert(varchar(255),isnull([l3cache],'''')) [CPUl3cache]
	,convert(varchar(255),[enabledcores]) [CPUenabledcores]
	,convert(varchar(255),count(1)) [CPUs]
FROM [dbo].[raw_Processor]
where [systemname] = ''@srv_name''
group by [systemname]
      ,[name]
      ,[cores]
      ,[logicalprocessors]
      ,[clockspeedmax]
      ,[clockspeedcurrent]
      ,[l2cache]
      ,[l3cache]
      ,[enabledcores]
) t
unpivot
(val for [Parameter] in
	(
	  --[systemname],
      [CPUname]
      ,[CPUcores]
      ,[CPUlogicalprocessors]
      ,[CPUclockspeedmax]
      ,[CPUclockspeedcurrent]
      ,[CPUl2cache]
      ,[CPUl3cache]
      ,[CPUenabledcores]
	  ,[CPUs]
	  )
) a 
option (recompile)
'

/*raw_SQLServices*/
set @q5=
'
select ''SQL Services #''+convert(varchar(20),row_number() over (order by displayname))
	,replace(displayname + '': ''+state +'' (''+startmode+'')'',''"'','''')
from [dbo].[raw_SQLServices]
where [systemname]=''@srv_name''
option (recompile)
'

/*sql info*/
set @q6=
'
select parameter, replace(val,''"'','''') [val]
from 
(select convert(varchar(255),[Instance]) [Instance]
	,convert(varchar(255),[Host]) [SQLHost]
	,convert(varchar(255),convert(varchar(20),convert(date,[Installed]),106)) [SQLInstalled]
	,convert(varchar(255),[Version]) [SQLVersion]
	,convert(varchar(255),[EngineEdition]) [SQLEngineEdition]
	,convert(varchar(255),[ProductVersion]) [SQLProductVersion]
	,convert(varchar(255),[ProductLevel]) [SQLProductLevel]
	,convert(varchar(255),[ProductUpdateLevel]) [SQLProductUpdateLevel]
	,convert(varchar(255),[Collation]) [SQLCollation]
	,convert(varchar(255),[Authentication]) [SQLAuthentication]
	,convert(varchar(255),[AuthScheme]) [SQLAuthScheme]
	,convert(varchar(255),[IsHADREnabled]) [SQLIsHADREnabled]
	,convert(varchar(255),[IsClustered]) [SQLIsClustered]
	,convert(varchar(255),[IsMirrored]) [SQLIsMirrored]
	,convert(varchar(255),[IsReplicated]) [SQLIsReplicated]
	,convert(varchar(255),[IsLogShipped]) [SQLIsLogShipped]
	,convert(varchar(255),
			case 
				when [uptime_mins]&gt;=1440 then convert(varchar(10),floor(uptime_mins/1440))+'' day(s) '' 
				else '''' 
			end 
			+convert(varchar(20),dateadd(mi,uptime_mins,0),108)
			) [SQLUptime]

FROM [dbo].[raw_SQLServer]
where [Instance] = ''@srv_name''

) t
unpivot
(val for [Parameter] in
	(
	 [SQLHost]
      ,[SQLInstalled]
      ,[SQLVersion]
      ,[SQLEngineEdition]
      ,[SQLProductVersion]
      ,[SQLProductLevel]
      ,[SQLProductUpdateLevel]
      ,[SQLCollation]
      ,[SQLAuthentication]
      ,[SQLAuthScheme]
      ,[SQLIsHADREnabled]
      ,[SQLIsClustered]
      ,[SQLIsMirrored]
      ,[SQLIsReplicated]
      ,[SQLIsLogShipped]
      ,[SQLUptime]
	  )
) a 
option (recompile)
'


while exists (select top 1 1 from #compare)
	begin
		select top 1 
			@srv=servername
		from #compare

		select @cmd1=replace(@q1,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		select @cmd1=replace(@q2,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		select @cmd1=replace(@q3,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		select @cmd1=replace(@q4,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		select @cmd1=replace(@q5,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		select @cmd1=replace(@q6,'@srv_name',@srv)
		insert into #inventory (parameter,val)
		exec(@cmd1)

		update #inventory 
		set srv=@srv
		where srv is null

		delete #compare
		where servername=@srv
	end

set @addon=''

if @show_diff=1 
	select @addon=' and isnull(t1.val,''[no data]'')&lt;&gt;isnull(t2.val,''[no data]'')'

;with counts as 
(select srv, count(1) [n] from #inventory group by srv)
select top 1 @srv=srv 
from counts 
order by n desc

select @cmd1='
select 
	isnull(t1.srv,''[no data]'') srv1
	,isnull(t1.parameter,t2.parameter) [parameter]
	,isnull(t1.val,''[no data]'') val1 
	,isnull(t2.val,''[no data]'') val2
	,isnull(t2.srv,''[no data]'') srv2
from #inventory t1
full outer join #inventory t2 on t1.parameter=t2.parameter and t1.srv&lt;&gt;t2.srv 
where t1.srv='''+@srv+''''+@addon+' option (recompile)'


exec (@cmd1)
</CommandText>
