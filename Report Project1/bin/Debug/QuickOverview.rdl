    <DataSet Name="ServerInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>declare @cmd varchar(2048)
select @cmd='
select 
	@@servername [SQLServer]
	, SERVERPROPERTY(''ComputerNamePhysicalNetBIOS'') [HostName]
	, convert(varchar(20), sqlserver_start_time,106)+ '' '' +convert(varchar(10), 	sqlserver_start_time,108) [Started]
	, Case When datediff(ss,sqlserver_start_time,getdate()) &gt;= 86400  
		     Then Convert(VarChar(5), datediff(ss,sqlserver_start_time,getdate())/86400) + '' days ''   
     Else '''' End  
	     + Convert(VarChar(8), DateAdd(Second, datediff(ss,sqlserver_start_time,getdate()), 0), 108) [SQLUptime]'+
	case 
		when convert(varchar(20),SERVERPROPERTY('ProductVersion')) like '10.0.%' then ',''see @@version'' [ServerType]' 
		else 
	', case 
		when virtual_machine_type_desc=''NONE'' then ''Physical''
		when virtual_machine_type_desc=''HYPERVISOR'' then ''Virtual'' 
		else virtual_machine_type_desc end [ServerType]' end 
	+
	', getdate() [ServerTime]
	, replace(replace(replace(@@version,char(10),''''),char(13),''''),''Corporation'', ''Corporation'') [SQLVersion]
	, (select convert(varchar(20),min(create_date),106) from sys.server_principals where name in (''RABONETEU\eu.mgt.DatabaseAdmins.ls'',''NT AUTHORITY\SYSTEM'')) [Installed]
from sys.dm_os_sys_info option (recompile)'
exec (@cmd)</CommandText>
      </Query>
