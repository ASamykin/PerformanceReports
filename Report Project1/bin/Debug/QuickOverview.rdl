    <DataSet Name="dsSuspectRepairPages">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>declare @tbl table (source varchar(50), errors int, lastdate datetime)

if object_id('sys.dm_db_mirroring_auto_page_repair') is not null 
	
	insert into @tbl (source, errors,lastdate)
	exec ('select ''Mirroring'',count(1),max(modification_time) from sys.dm_db_mirroring_auto_page_repair')
else 
	insert into @tbl (source,errors,lastdate)
	values ('Mirroring',0,NULL)

if object_id('sys.dm_hadr_auto_page_repair') is not null 
	
	insert into @tbl (source,errors,lastdate)
	exec ('select ''HADR'',count(1),max(modification_time) from sys.dm_hadr_auto_page_repair')
else 
	insert into @tbl (source,errors,lastdate)
	values ('HADR',0,NULL)


if object_id('msdb.dbo.suspect_pages') is not null 
	
	insert into @tbl (source,errors,lastdate)
	exec ('select ''Suspect'',count(1),max(last_update_date) from msdb.dbo.suspect_pages ')
else 
	insert into @tbl (source,errors,lastdate)
	values ('Suspect',0,NULL)

select source,errors, lastdate 
from @tbl
</CommandText>
      </Query>
      <Fields>
