          <DataField>wait_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum_wait_time_ms">
          <DataField>sum_wait_time_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="pct_wait_time">
