    <DataSet Name="TracesInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*select count(1) [Count] from sys.traces */
with data as (
select 
	case 
		when is_default=1 then 'system' 
		else 'user' 
	end [tracetype]
	,count(1) [sessions]
	,case 
		when status=1 then 'Running'
		when status=0 then 'Stopped'
		else 'Unknown' 
	end [Status]
from sys.traces 
group by is_default, status
)
select sum(d1.[sessions]) [Count]
	--,@msg=@msg+char(10)+convert(varchar(10),[sessions])+' '+tracetype+' ('+[status]+')'
	,(select char(10)+convert(varchar(10),d2.[sessions])+' '+d2.tracetype+' ('+d2.[status]+')' as 'data()' from data d2 for xml path('')) [msg]
from data d1
option (recompile,maxdop 1)</CommandText>
      </Query>
