    <DataSet Name="dsBackupStats">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with backups as 
	(select 
		server_name +' '+ quotename(convert(varchar(20),count(1))) [name]
	from msdb..backupset
	where server_name&lt;&gt;@@servername
	group by server_name 
	)
select 
	min(backup_start_date) [BackupMinDate]
	,max(backup_start_date) [BackupMaxDate]
	,datediff(dd,min(backup_start_date),max(backup_start_date)) [BackupDaysHistory]
	,(select count(1) from msdb..backupset where server_name&lt;&gt;@@servername ) [BackupImportedHistory]
	,stuff((select ', '+name as 'data()' from backups for xml path('')),1,2,'') [BackupFromServers]
