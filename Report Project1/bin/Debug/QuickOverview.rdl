    <DataSet Name="dsPlanCache_Details2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with SizeLeaders as (
select 
	ROW_NUMBER() over (partition by objtype,cacheobjtype order by size_in_bytes desc) n
	,objtype,cacheobjtype,size_in_bytes
	,usecounts,refcounts,plan_handle
from sys.dm_exec_cached_plans 
)
, OtherSameSize as 
(select 
	objtype,cacheobjtype,size_in_bytes,count(1) [amount]
from sys.dm_exec_cached_plans 
group by objtype,cacheobjtype,size_in_bytes
)
select 
	b.objtype
	,b.cacheobjtype
	,b.size_in_bytes/1024 [sizeKB]
	,b.usecounts
	,b.refcounts
	,d.amount [PlansSameSize]
	,case when t.dbid=32767 then 'mssqlsystemresource' else db_name(t.dbid) end [db]
	,isnull(object_name(t.objectid,t.dbid),left(t.text,200)) [ObjectQuery]
from SizeLeaders b
join OtherSameSize d on (b.objtype=d.objtype and b.cacheobjtype=d.cacheobjtype and b.size_in_bytes=d.size_in_bytes)
outer apply sys.dm_exec_sql_text(b.plan_handle) t
where b.n=1
option (recompile,maxdop 1)</CommandText>
      </Query>
      <Fields>
        <Field Name="objtype">
          <DataField>objtype</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cacheobjtype">
