    <DataSet Name="dsRestoreStats">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with restores as 
	(
	select server_name +' '+quotename(convert(varchar(20),count(1))) [srv]
	from  msdb.dbo.backupset 
	where backup_set_id in (select backup_set_id from msdb.dbo.restorehistory)
	group by server_name
	)
select 
	min(rh.restore_date) [FirstRestore]
	,max(rh.restore_date) [LastRestore]
	,datediff(dd,max(rh.restore_date),getdate()) [LastRestoreDaysAgo]
	,count(1) [Restores]
	,stuff((select ', '+ srv as 'data()' from restores for xml path('')),1,2,'') [SourceServer] 
from msdb.dbo.restorehistory rh
option (recompile,maxdop 1)</CommandText>
