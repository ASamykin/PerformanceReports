    <DataSet Name="Deadlocks">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
with dls(dt) as
(
SELECT 
   event_data.value('(/event/@timestamp)[1]','datetime') AS TS1
FROM 
(   SELECT XEvent.query('.') AS event_data 
    FROM 
    (   -- Cast the target_data to XML 
        SELECT CAST(target_data AS XML) AS TargetData 
        FROM sys.dm_xe_session_targets AS st 
        INNER JOIN sys.dm_xe_sessions AS s 
            ON s.address = st.event_session_address 
        WHERE name = N'system_health' 
          AND target_name = N'ring_buffer' 
    ) AS Data 
    -- Split out the Event Nodes 
    CROSS APPLY TargetData.nodes ('RingBufferTarget/event[@name="xml_deadlock_report"]') AS XEventData (XEvent)   
) AS tab (event_data))
select count(1) [DLsToday]
from dls 
where dt&gt;=convert(datetime,convert(varchar(20),getdate(),112))
option (recompile)
*/

IF convert(varchar(200),@@VERSION) like '% - 10.50.%' 
   with dls(dt) as
(
SELECT 
   event_data.value('(/event/@timestamp)[1]','datetime') AS TS1
FROM 
(   SELECT XEvent.query('.') AS event_data 
    FROM 
    (   -- Cast the target_data to XML 
        SELECT CAST(target_data AS XML) AS TargetData 
        FROM sys.dm_xe_session_targets AS st 
        INNER JOIN sys.dm_xe_sessions AS s 
            ON s.address = st.event_session_address 
        WHERE name = N'system_health' 
          AND target_name = N'ring_buffer' 
    ) AS Data 
    -- Split out the Event Nodes 
    CROSS APPLY TargetData.nodes ('RingBufferTarget/event[@name="xml_deadlock_report"]') AS XEventData (XEvent)   
) AS tab (event_data))
select 
 isnull(sum(case when dateadd(dd,datediff(dd,'20100101',dt),'20100101')=dateadd(dd,datediff(dd,'20100101',getdate()),'20100101') then 1 else 0 end),0) [DLsToday]
 ,isnull(sum(case when dateadd(dd,datediff(dd,'20100101',dt),'20100101')&lt;dateadd(dd,datediff(dd,'20100101',getdate()),'20100101') then 1 else 0 end),0) [DLsBefore]

from dls 
--where dt&gt;=convert(datetime,convert(varchar(20),getdate(),112))
option (recompile)
else 
begin
	declare @file varchar(4000)
	select @file=reverse(stuff(reverse([path]),1,CHARINDEX('\',reverse([path]),1)-1,''))+'system_health*.xel'
	from sys.traces where id=1

	begin try
		;with dls as (
		SELECT 
				dateadd(hh,datediff(hh,getutcdate(),getdate()),convert(datetime,CONVERT(xml, event_data).value('(event[@name="xml_deadlock_report"]/@timestamp)[1]','datetime'))) as  [dt]
				,CONVERT(xml, event_data).query('/event/data/value/child::*') [Deadlock]
		FROM sys.fn_xe_file_target_read_file(@file, null,  null, null)
		WHERE object_name like 'xml_deadlock_report'
		)
		select 
		 isnull(sum(case when dateadd(dd,datediff(dd,'20100101',dt),'20100101')=dateadd(dd,datediff(dd,'20100101',getdate()),'20100101') then 1 else 0 end),0) [DLsToday]
		 ,isnull(sum(case when dateadd(dd,datediff(dd,'20100101',dt),'20100101')&lt;dateadd(dd,datediff(dd,'20100101',getdate()),'20100101') then 1 else 0 end),0) [DLsBefore]
		from dls 
		option (recompile)
	end try

	begin catch
		print ERROR_MESSAGE()
	end catch
end</CommandText>
      </Query>
      <Fields>
        <Field Name="DLsToday">
          <DataField>DLsToday</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DLsBefore">
          <DataField>DLsBefore</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
