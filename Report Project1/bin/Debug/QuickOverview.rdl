    <DataSet Name="dsErrorlogDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>set nocount on;
declare @tbl table 
(dt datetime,proc_info varchar(30),msg varchar(512) )
declare @dt date = getdate()
declare @defect tinyint = 0

if left(convert(varchar(20),SERVERPROPERTY('ProductVersion')),3)='10.'
	set @defect=1  /* on SQL2008&amp;R2 xp_readerrorlog doesn't filter by date and return all rows*/

insert into @tbl (dt,proc_info,msg)
exec xp_readerrorlog 0,1,N'Error: ',null,@dt 

if @defect=1
	delete @tbl 
	where dt&lt;@dt
		or msg not like 'Error: %'

------------------------------------
insert into @tbl (dt,proc_info,msg)
exec xp_readerrorlog 0,1,N'taking longer',null,@dt 

if @defect=1
	delete @tbl 
	where dt&lt;@dt
		or msg not like '%taking longer%'

if exists (select top 1 1 from @tbl where msg like '%taking longer%')
	begin
		insert into @tbl (dt,proc_info,msg)
		select 
			@dt,
			'system'
			,convert(varchar(10),sum(convert(int,left(replace(msg,'SQL Server has encountered ',''),charindex(' occur',msg)-len('SQL Server has encountered ')-2)))) 
				+' I/O delays on file '+SUBSTRING(msg,charindex('[',msg),charindex(']',msg)-charindex('[',msg)+1)
				+' of database '
				+case
					when left(stuff(msg,1,charindex(' in database ',msg),''),500) like '%(%)%' then substring(msg,charindex(' in database ',msg)+len(' in database '),charindex('.  The OS',msg)-charindex(' in database ',msg)-len(' in database '))
					else db_name(substring(msg,charindex('in database id',msg)+len('in database id')+1,charindex('.  The OS file',msg)-charindex('in database id',msg)-len('in database id')-1))
					end 
				+'.' [msg]
		from @tbl 
		where msg like '%taking longer%'
		group by [msg]

		delete @tbl
		where msg like '%taking longer%'
	end
--------------------

insert into @tbl (dt,proc_info,msg)
exec xp_readerrorlog 0,1,N'non-yielding',null,@dt 

if @defect=1
	delete @tbl 
	where dt&lt;@dt
		or msg not like '%non-yielding%'

if exists (select top 1 1 from @tbl where msg like '%non-yielding%')
	begin
		insert into @tbl (dt,proc_info,msg)
		select @dt,'system',convert(varchar(10),count(1)) +' non-yielding process(-es) detected on Scheduler X.' 
		from @tbl 
		where msg like '%appears to be non-yielding%'
		group by [msg]

		delete @tbl
		where msg like '%appears to be non-yielding%'
		
	end

--------------------
;with logged as (
select 
	case 
		when msg not like 'Error:%Severity:%State:%' then -1
		else SUBSTRING(msg,charindex(' ',msg,1)+1,charindex(',',msg,1)-charindex(' ',msg,1)-1)
	end  [ErrorNum]
	,left(msg,100) [ErrorText]
	,count(1) [Amount]
from @tbl
group by 
case 
		when msg not like 'Error:%Severity:%State:%' then -1
		else SUBSTRING(msg,charindex(' ',msg,1)+1,charindex(',',msg,1)-charindex(' ',msg,1)-1)
	end 
	,left(msg,100)
)
select l.ErrorNum
	,l.Amount
	,isnull(m.text,'[Unknown] '+l.ErrorText) [text]
from logged l 
left join sys.messages m on l.ErrorNum=m.message_id and language_id=1033</CommandText>
      </Query>
      <Fields>
        <Field Name="ErrorNum">
          <DataField>ErrorNum</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="text">
          <DataField>text</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
