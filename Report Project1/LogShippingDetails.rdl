    <DataSet Name="LS_SecondaryDBs">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	row_number() over (order by ms.secondary_database) [N]
	,ms.primary_server
	,ms.primary_database [SourceDB]
	,ms.secondary_server 
	,ms.secondary_database [TargetDB]
	,ms.restore_threshold [RestoreThreshold_min]
	,case 
		when ms.threshold_alert_enabled=1 then 'Enabled' 
		else 'Disabled' end [AlertStatus]

	--,ms.last_copied_file
	, replace(ms.last_copied_file,reverse(left(REVERSE(ms.last_copied_file),charindex('\',REVERSE(ms.last_copied_file))-1)),'') [CopiedFrom]
	, reverse(left(REVERSE(ms.last_copied_file),charindex('\',REVERSE(ms.last_copied_file))-1)) [LastCopiedFile]
	,ms.last_copied_date

	--,ms.last_restored_file
	, replace(ms.last_restored_file,reverse(left(REVERSE(ms.last_restored_file),charindex('\',REVERSE(ms.last_restored_file))-1)),'') [RestoredFrom]
	, reverse(left(REVERSE(ms.last_restored_file),charindex('\',REVERSE(ms.last_restored_file))-1)) [LastRestoredFile]

	,ms.last_restored_date
	,ms.last_restored_latency [CreateRestoreLatency_min]
	,ms.history_retention_period/60 [HistoryRetention_hours] 
from msdb..log_shipping_monitor_secondary  ms  (NOLOCK)
option (recompile)</CommandText>
      </Query>
