  <DataSets>
    <DataSet Name="LS_PrimaryDBs">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select  
	row_number() over (order by pd.primary_database) [N]
    ,mp.primary_server
    ,pd.primary_database
	,lsps.secondary_database
	,lsps.secondary_server
	/*Backup*/
    ,pd.backup_directory
    ,pd.backup_share
    ,replace(mp.last_backup_file,pd.backup_directory,'...') [LastBackupFile]
    ,mp.last_backup_date
	,Case When datediff(ss,mp.last_backup_date,getdate()) &gt;= 86400
				Then Convert(VarChar(5), datediff(ss,mp.last_backup_date,getdate())/86400) 
				+ ' days ' 
				Else ''
				End
				+ Convert(VarChar(8), DateAdd(Second, datediff(ss,mp.last_backup_date,getdate()), 0), 108) [TimePassed]
    ,mp.backup_threshold [BackupWarning_min]

    ,mp.history_retention_period/60 [HistoryRetention_hours]
    ,pd.backup_retention_period/60 [BackupRetention_hours]

    ,case 
		when	pd.backup_compression = 0 then 'Disabled'
		when	pd.backup_compression = 1 then 'Enabled'
		when	pd.backup_compression = 2 then 'Server default' 
		else	convert(varchar(10),pd.backup_compression) end [BackupCompression]
	/*Backup Job*/
	,sj.name [BackupJob]
	,case when sj.enabled=1 then 'Enabled' else 'Disabled' end [JobStatus]
	,sj.date_created [JobCreated]
	,sj.date_modified [JobModified]

	/*Alert*/
    ,mp.threshold_alert [AlertID]
    ,case 
		when mp.threshold_alert_enabled=0 then 'Disabled'
		when mp.threshold_alert_enabled=1 then 'Enabled' end [ThresholdAlertStatus]
	/*Monitor*/
    --,'monitor_server' = case when (pd.user_specified_monitor = 1) then pd.monitor_server else cast(NULL as sysname) end
	,'monitor_server' = pd.monitor_server
    ,'monitor_server_security_mode' = case when (pd.user_specified_monitor = 1) then pd.monitor_server_security_mode else cast(NULL as bit) end

from msdb.dbo.log_shipping_primary_databases as pd (NOLOCK)
		join msdb.dbo.log_shipping_monitor_primary as mp (NOLOCK) on pd.primary_id = mp.primary_id
		join msdb.dbo.sysjobs as sj  (NOLOCK) on sj.job_id=pd.backup_job_id
		join msdb.dbo.log_shipping_primary_secondaries as lsps (NOLOCK) on lsps.primary_id=pd.primary_id
order by pd.primary_database
option (recompile)</CommandText>
      </Query>
