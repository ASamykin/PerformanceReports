    <DataSet Name="dsDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@l">
            <Value>=Parameters!pUser.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@h">
            <Value>=Parameters!pHost.Value</Value>
