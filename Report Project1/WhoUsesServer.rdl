  <DataSets>
    <DataSet Name="dsDefaultTrace">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*Note:
this query populates temporary table #WhoUsesInfo which is being used by the rest datasets*/

if object_id('tempdb..##WhoUsesInfo') is not null    /* reuse previously collected data to speed-up the report*/
	begin
		exec ('if (select top 1 datediff(mi,collected,getdate()) from ##WhoUsesInfo)&gt;5 drop table ##WhoUsesInfo')
		if object_id('tempdb..##WhoUsesInfo') is not null 
			return
	end 


declare @all_files varchar(200)

/* prepare path for all trace files */
select @all_files=CAST(value AS VARCHAR(200))
FROM sys.fn_trace_getinfo(DEFAULT)
WHERE traceid = 1 AND property = 2

select @all_files=left(@all_files,len(@all_files)-charindex('_',reverse(@all_files)))+right(@all_files,4);

with data as (
SELECT	
		isnull(ft.LoginName,'(n/a)')  [LoginName]
		,isnull(ft.Hostname,'(n/a)')  [Hostname]
		,isnull(ft.ApplicationName,'(n/a)') [ApplicationName]
		,isnull(ft.DatabaseName,'(n/a)') [DatabaseName]
		,convert(date,ft.StartTime) [Date]
		,te.name [EventType]
		,count(1) [Count]
FROM sys.traces st CROSS Apply
	::fn_trace_gettable(@all_files, st.max_files) AS ft 
INNER JOIN sys.trace_events AS te ON ft.EventClass = te.trace_event_id 
WHERE 
--ft.LoginName is not null and 
st.is_default=1 
and st.status=1

group by 
ft.LoginName
		,ft.Hostname
		,ft.ApplicationName
		,ft.DatabaseName
		,te.name
		,convert(date,ft.StartTime)
UNION ALL
select 
	loginame [LoginName]
	,hostname [Hostname]
	,program_name [ApplicationName]
	,db_name(dbid) [DatabaseName]
	,convert(date,login_time) [DT]
	,'Connection' [EventType]
	,count(1) [Count]
from sysprocesses 
where spid&gt;50

group by 
	loginame
	,hostname
	,program_name
	,db_name(dbid)
	,convert(date,login_time)
)
select 
	distinct 
getdate() [collected]
		,LoginName
		,Hostname
		,ApplicationName
		,DatabaseName
		,[Date]
		,stuff((select ', '+isnull(t2.EventType,'')+' '+quotename(isnull([Count],'')) as 'data()' 
			from data t2 
			where 
				t2.LoginName=t1.LoginName 
			and t2.HostName=t1.HostName
			and t2.ApplicationName=t1.ApplicationName
			and t2.DatabaseName=t1.DatabaseName
			and t2.[Date]=t1.[Date]
			for xml path('')),1,2,'') [Events]
, case 
			when ApplicationName like 'SQLAgent - TSQL JobStep%' then replace(replace(replace(ApplicationName,'SQLAgent - TSQL JobStep (Job ',''),'Step ',''),')','') else '' end [Job]
into ##WhoUsesInfo
from data t1
option (recompile)</CommandText>
      </Query>
