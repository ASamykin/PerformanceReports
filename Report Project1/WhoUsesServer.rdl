    <DataSet Name="dsEventsPerDay">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with dates as 
	(
	select 
		min(Date) [min]
		,max(Date) [max]
		,datediff(dd,min(Date),max(Date)) [days]
	from ##WhoUsesInfo
	)
,all_dates as 
	(
		select convert(smallint,0) [n], [min] [dt], [days] 
		from dates 
		union all
		select  convert(smallint,n+1),dateadd(dd,1,[dt]),[days]
		from all_dates 
		where [days]&gt;=n+1
		
	)
--select * from all_dates 
select 
	isnull(w.Date,ad.dt) [dt]
	,isnull(count(*),0) [events]
 from ##WhoUsesInfo w
 right join all_dates ad on w.[Date]=ad.dt
where ad.dt&lt;=convert(date,getdate())
group by isnull(w.Date,ad.dt)
OPTION (MAXRECURSION 1000, recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="events">
          <DataField>events</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsJobs">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	--j.job_id
	--,j.name 
	--,js.step_id
	--,js.step_name
	--,js.subsystem,
	convert(varchar(200),j.job_id)+' : '+convert(varchar(10),js.step_id) [f1]
	,'SQL Job "'+j.name+'", Step '+convert(varchar(10),js.step_id)+' "'+js.step_name+'" '+quotename(js.subsystem) [f2]
from msdb..sysjobs j
join msdb..sysjobsteps js on j.job_id=js.job_id</CommandText>
      </Query>
      <Fields>
