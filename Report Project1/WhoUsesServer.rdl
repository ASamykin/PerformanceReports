          </QueryParameter>
          <QueryParameter Name="@a">
            <Value>=Parameters!pApplication.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@d">
            <Value>=Parameters!pDatabase.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dt">
            <Value>=Parameters!pDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	LoginName
	,HostName
	,ApplicationName
	,DatabaseName
	,[Date]
	,[Events]
from ##WhoUsesInfo

where 
	LoginName like '%'+@l+'%'
	and HostName like '%'+@h+'%'
	and ApplicationName like '%'+@a+'%'
	and DatabaseName like '%'+@d+'%'
	and [Date] = isnull(@dt,[Date])

order by [Date] desc
option (recompile)</CommandText>
      </Query>
