          <QueryParameter Name="@db">
            <Value>=Parameters!pDatabase.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd varchar(1000)
set @cmd = 'select distinct Hostname  from ##WhoUsesInfo where 1=1 '

     +case when @dt is not null then ' and [Date] ='''+@dt+''''  else '' end
 
     +case when @app&lt;&gt;'%'  then ' and [ApplicationName] ='''+@app+''''  else '' end
     +case when @usr&lt;&gt;'%'   then ' and [LoginName] ='''+@usr+''''  else '' end
     +case when @db&lt;&gt;'%'    then ' and [DatabaseName] ='''+@db+''''  else '' end
     + ' option (recompile)'

exec (@cmd)</CommandText>
      </Query>
      <Fields>
        <Field Name="HostName">
          <DataField>HostName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
