  <DataSets>
    <DataSet Name="dsSQLInstances">
      <Query>
        <DataSourceName>SQLInventory</DataSourceName>
        <CommandText>with src as 
(
select left([Principal Name],charindex('.',[Principal Name])-1) + case when [Instance Name] ='MSSQLSERVER' then '' else '\'+[Instance Name] end [SQL]
	,*
	from tbInstanceDescription
)
select 

	src.[Functional Owner]
	,src.[Instance Description]
	,src.[Environment]
	,si.instance
	,si.Installed
	,datename(mm,si.Installed)+ ' ' + convert(varchar(10),datepart(yy,si.Installed)) [mm]
	,'Q'+convert(varchar(10),datepart(qq,si.Installed))+' '+convert(varchar(10),datepart(yy,si.Installed)) [qq]
	,datepart(yy,si.Installed) [yy]
	,case when charindex('-',si.Version)&lt;&gt;0 then left (si.Version,charindex('-',si.Version)-2) else si.Version end [Version]
	,si.EngineEdition

from raw_SQLServer si
left join src on si.[Instance]=src.[SQL]
order by si.Instance 
</CommandText>
      </Query>
      <Fields>
        <Field Name="Functional_Owner">
          <DataField>Functional Owner</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Instance_Description">
