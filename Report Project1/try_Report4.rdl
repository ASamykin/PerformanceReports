  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
this query returns less data than there are in sys.dm_exec_cached_plans 
*/
select 
convert(varchar(20),dateadd(dd,datediff(dd,0,s.creation_time),0),106) [day]
,left(convert(varchar(10),dateadd(hh,datediff(hh,0,s.creation_time),0),108),5) [dt]
,p.objtype+':'+p.cacheobjtype [type]
,count(distinct s.plan_handle) [plans]
from sys.dm_exec_query_stats s
join sys.dm_exec_cached_plans p on s.plan_handle=p.plan_handle
group by 
dateadd(hh,datediff(hh,0,s.creation_time),0)
,dateadd(dd,datediff(dd,0,s.creation_time),0) 
,p.objtype+':'+p.cacheobjtype
option (recompile, maxdop 1)</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="day">
          <DataField>day</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="plans">
          <DataField>plans</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
