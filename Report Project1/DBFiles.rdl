    <DataSet Name="dsUsage">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ddrive">
            <Value>=Parameters!DiskDrive.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @ddrive varchar(50)  = 'I:\SQL_Log\';
declare @cmd varchar(8000) = '';
declare @used numeric(10,2) = 0
declare @out varchar(1024) = ''  -- dynamic filter 
declare @tmp table (ttl numeric(10,2))

if len(@ddrive)=3  /*valid for real disk drives; exclude mount points which reside on the drive*/
	select @out=@out+' and physical_name not like '''+volume_mount_point+'%'' ' 
	from #total 
	where volume_mount_point &lt;&gt;@ddrive 

set @cmd='
select convert(numeric(10,2),sum(size/128.)/1024) /*in GBs*/
from sys.master_files 
where physical_name like '''+@ddrive+ '%''' +@out + ' option (recompile);'

insert into @tmp (ttl)
exec (@cmd)

-- get the total size of all files
select top 1 @used=isnull(ttl,0) from @tmp

select 
	convert(numeric(10,2),Total_Size_GB) [total]
	,convert(numeric(10,2),Available_Size_GB) [free]
	,@ddrive [drive]
	,@ddrive [filter]  -- will not be used
	,convert(numeric(10,2),@used) [usedbyfiles]
from #total
where volume_mount_point=@ddrive

drop table #total</CommandText>
      </Query>
      <Fields>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="free">
          <DataField>free</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="drive">
          <DataField>drive</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="filter">
          <DataField>filter</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="usedbyfiles">
          <DataField>usedbyfiles</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
