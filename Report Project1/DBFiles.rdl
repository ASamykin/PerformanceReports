        </Field>
        <Field Name="SpaceFreeMB">
          <DataField>SpaceFreeMB</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="MaxSizeMB">
          <DataField>MaxSizeMB</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Growth">
          <DataField>Growth</DataField>
