    <DataSet Name="dsDisks">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

declare @drives table (drive varchar(5),MBFree int)
declare @drive_info table (
	volume_mount_point nvarchar(512),
	file_system_type nvarchar(512),
	logical_volume_name nvarchar(512),
	[Total_Size_GB] DECIMAL(18,2),
	[Available_Size_GB] DECIMAL(18,2),
	[Space_Free_Percent] DECIMAL(18,2))


insert into @drives (drive,MBFree)
exec xp_fixeddrives 

update @drives 
set drive=drive+':\'

--select * from @drives 

if convert(varchar(20),SERVERPROPERTY('ProductVersion')) not like '10.0.%' 
	begin
		declare @cmd varchar(2048)
		select @cmd=
					'with files(database_id,file_id) as
						(select 
							min(database_id)
							,min(file_id)
						from sys.master_files with (NOLOCK)
						group by 
						left(physical_name,len(physical_name)-charindex(''\'',reverse(physical_name)))
						)
					SELECT distinct
						vs.volume_mount_point
					, vs.file_system_type
					, vs.logical_volume_name
					, CONVERT(DECIMAL(18,2),vs.total_bytes/1024/1024/1024.0) AS [Total_Size_GB]
					, CONVERT(DECIMAL(18,2),vs.available_bytes/1024/1024/1024.0) AS [Available_Size_GB]
					, CAST(CAST(vs.available_bytes AS FLOAT)/ CAST(vs.total_bytes AS FLOAT) AS DECIMAL(18,2)) * 100 AS [Space_Free_Percent] 
					FROM files AS f WITH (NOLOCK)
					CROSS APPLY sys.dm_os_volume_stats(f.database_id, f.[file_id]) AS vs 
					OPTION (RECOMPILE)'
		--select @cmd
		insert into @drive_info([volume_mount_point],[file_system_type],[logical_volume_name],[Total_Size_GB],[Available_Size_GB],[Space_Free_Percent])
		exec (@cmd)
	end 

;with data as (
select 
	di.[volume_mount_point] [volume_mount_point]
	,convert(varchar(50),di.[file_system_type]) [file_system_type]
	,convert(varchar(50),di.[logical_volume_name]) [logical_volume_name]
	,convert(varchar(50),di.[Total_Size_GB]) [Total_Size_GB]
	,convert(varchar(50),di.[Available_Size_GB]) [Available_Size_GB]
	,convert(varchar(50),di.[Space_Free_Percent]) [Space_Free_Percent]
	,type_desc+' ('+convert(varchar(10),count(1))+')' [dbfiles]
from @drive_info di
join sys.master_files mf  on patindex(di.volume_mount_point+'%',mf.physical_name)&gt;0
group by di.volume_mount_point
	,convert(varchar(50),di.[file_system_type])
	,convert(varchar(50),di.[logical_volume_name]) 
	,convert(varchar(50),di.[Total_Size_GB])
	,convert(varchar(50),di.[Available_Size_GB])
	,convert(varchar(50),di.[Space_Free_Percent])
	,mf.type_desc
)
select distinct 
	 d1.[volume_mount_point]
	,d1.[file_system_type]
	,d1.[logical_volume_name]
	,d1.[Total_Size_GB]
	,d1.[Available_Size_GB]
	,d1.[Space_Free_Percent]
	,100-convert(numeric(8,2),d1.[Space_Free_Percent]) [Space_Used_Percent]
	,isnull(reverse(stuff(reverse((select d2.dbfiles + ', ' as 'data()' from data d2 where d2.[volume_mount_point]=d1.[volume_mount_point] for xml path('') )),1,2,'')),'') [Type&amp;Amount]
into #total
from data d1 
union all
select 
	drive,NULL,NULL,NULL,convert(varchar(20),convert(numeric(10,2),d.MBFree/1024.)),NULL,NULL,NULL
from @drives d
where drive not in (select [volume_mount_point] from data)
option (recompile)

select 
[volume_mount_point]
,[file_system_type]
,[logical_volume_name]
,[Total_Size_GB]
,[Available_Size_GB]
,[Space_Free_Percent]
,[Space_Used_Percent]
,[Type&amp;Amount]
from #total</CommandText>
      </Query>
